    ra "Tak zrobię. Dziękuję [p_name]."

# game/episode004_conversations.rpy:940
translate polish ep004_ziv_talk_8612aee8:

    # zi "About before, with Raene..."
    zi "Jeśli chodzi o to wcześniejsze, z Raene..."

# game/episode004_conversations.rpy:941
