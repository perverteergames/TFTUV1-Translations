translate portuguese ep004_aven_talk_e06760aa:

    # av "[p_name]?"
    av "[p_name]?"

# game/episode004_conversations.rpy:412
translate portuguese ep004_aven_talk_d35c2632:

    # c "Shit...{w} I..."
    c "Merda...{w} eu..."

# game/episode004_conversations.rpy:414
translate portuguese ep004_aven_talk_1a3c72af:

    # av "You're staring."
    av "Você está encarando."

# game/episode004_conversations.rpy:415
translate portuguese ep004_aven_talk_a1a205de:

    # av "Never seen cleavage up close?"
    av "Nunca viu decote de perto?"

# game/episode004_conversations.rpy:416
translate portuguese ep004_aven_talk_596f6da5:

    # c "I have...{w} I mean...{w} I'll leave."
    c "Eu tenho..{w} digo...{w} vou sair."

# game/episode004_conversations.rpy:417
translate portuguese ep004_aven_talk_eb86e3e5:

    # av "Guess you didn't do the communal shower thing at the Academy?"
    av "Imagino que vocês não tinham banheiros mistos na Academia?"

# game/episode004_conversations.rpy:418
translate portuguese ep004_aven_talk_cd8b2dde:

    # c "No, we didn't."
    c "Não, não tínhamos."

# game/episode004_conversations.rpy:420
translate portuguese ep004_aven_talk_84c554dc:

    # av "Well, I saw some things back on Skarak, so showing you a bit of skin isn't going to harm anybody."
