    # centered "{=chapter_heading}EPISODE 11{/=chapter_heading}"
    centered "{=chapter_heading}EPISODE 11{/=chapter_heading}"

# game/episode011.rpy:36
translate german episode011_5d39ddd5:

    # er "Behold, Her Majesty, the Virgin Queen."
    er "Seht, Ihre Majestät, die Jungfräuliche Königin."

# game/episode011.rpy:38
translate german episode011_7cedee09:

    # e "We are gathered here today to celebrate the induction of a Hero of the Acarhyn into the Annals of Our History."
    e "Wir sind heute hier versammelt, um die Aufnahme eines Helden von Acarhyn in die Annalen unserer Geschichte zu feiern."

# game/episode011.rpy:39
translate german episode011_685dfb13:

    # e "Please, step forward, [p_name]."
    e "Bitte, treten Sie vor, [p_name]."

# game/episode011.rpy:41
translate german episode011_d9b321c0:

    # er "The Hunters Council has deemed this Hunters trophy to be genuine."
    er "Der Jägerrat hat diese Jägertrophäe als echt eingestuft."

# game/episode011.rpy:42
translate german episode011_cfb3ffe2:

    # er "This man has single-handedly felled an Undrea."
    er "Dieser Mann hat im Alleingang einen Undrea erlegt."

# game/episode011.rpy:44
translate german episode011_b6121e71:

    # "I was glad to finally put a name to the monstrosity I’d killed."
    "Ich war froh, dem Monstrum, das ich getötet hatte, endlich einen Namen geben zu können."

# game/episode011.rpy:45
translate german episode011_d65f659a:

    # "Also, the feat of butchering the beast elicited some approving sounds from the audience, which bolstered my self-esteem quite a bit."
    "Auch die Tatsache, dass ich das Tier zerlegt hatte, rief einige zustimmende Laute aus dem Publikum hervor, was mein Selbstwertgefühl erheblich steigerte."

# game/episode011.rpy:58
translate german episode011_f77d95e8:

    # er "Will you accept this man to be a Hero of the Acarhyn?"
    er "Wirst du diesen Mann als Helden von Acarhyn anerkennen?"

# game/episode011.rpy:60
translate german episode011_33285b0b:

    # e "We wholeheartedly accept."
    e "Wir nehmen das uneingeschränkt an."

# game/episode011.rpy:63
translate german episode011_93880b79:

    # er "Let it be known that [p_name] Valenmann de Lonval, Brother to the Queen, will live on in the Annals of Acarhyn History as the Hero who single-handedly slayed an Undrea in the Year 849 since our Creation."
    er "Hiermit soll bekannt gegeben werden, dass [p_name] Valenmann de Lonval, Bruder der Königin, in den Annalen der Geschichte von Acarhyn als der Held weiterleben wird, der im Jahr 849 seit unserer Schöpfung im Alleingang einen Undrea erschlug."

# game/episode011.rpy:66
translate german episode011_9f1e9cdd:

    # rah "Cheers to the Hero of the Acarhyn and Brother to the Queen!"
    rah "Ein Hoch auf den Helden von Acarhyn und Bruder der Königin!"

# game/episode011.rpy:68
translate german episode011_31a384d4:

    # er "Let it be known that [p_name] Valenmann de Lonval, will live on in the Annals of Acarhyn History as the Hero who single-handedly slayed an Undrea in the Year 849 since our Creation."
    er "Hiermit soll bekannt gegeben werden, dass [p_name] Valenmann de Lonval in den Annalen der Geschichte von Acarhyn als der Held weiterleben wird, der im Jahr 849 seit unserer Schöpfung im Alleingang einen Undrea erschlug."

# game/episode011.rpy:71
translate german episode011_8456beaa:

    # rah "Cheers to the Hero of the Acarhyn!"
    rah "Ein Hoch auf den Helden von Acarhyn!"

# game/episode011.rpy:75
translate german episode011_72736542:

    # "Rahia stood firmly next to me as the crowd went wild."
    "Rahia stand fest neben mir, während die Menge tobte."

# game/episode011.rpy:76
translate german episode011_d8b36c58:

    # "The crowd went wild, despite Erylin’s less than riveting performance."
    "Die Menge tobte, trotz Erylins eher wenig mitreißender Darbietung."

# game/episode011.rpy:79
translate german episode011_5b1f9707:

    # "Erylin stood firmly next to me as the crowd went wild."
    "Erylin stand fest neben mir, während die Menge tobte."

# game/episode011.rpy:82
translate german episode011_c498988b:

    # "I stood rather alone on the podium as the crowd went wild."
    "Ich stand ziemlich allein auf dem Podium, während die Menge tobte."

# game/episode011.rpy:84
translate german episode011_dc912a82:

    # "I stole a glance at Eva and she smiled at me."
    "Ich warf Eva einen verstohlenen Blick zu und sie lächelte mich an."

# game/episode011.rpy:85
translate german episode011_d5b8d9e1:

    # "I wondered how much clout this new title would give me, but decided to try the possible benefits of my new station at the earliest convenience."
    "Ich fragte mich, wie viel Einfluss mir dieser neue Titel verschaffen würde, beschloss aber, die möglichen Vorteile meiner neuen Position so bald wie möglich auszuprobieren."

# game/episode011.rpy:88
translate german episode011_9ccda3fc:

    # rah "Tonight we’ll feast and celebrate the removal of the Undrea’s blight upon Acarhyn lands."
    rah "Heute Abend werden wir ein Festmahl veranstalten und die Beseitigung der Undrea-Plage über Acarhyn feiern."

# game/episode011.rpy:91
translate german episode011_c943f04b:

    # er "The extermination of the Undrea’s blight upon the Acarhyn lands has ended!"
    er "Die Ausrottung der Undrea-Plage in den Landen von Acarhyn ist beendet!"

# game/episode011.rpy:94
translate german episode011_c243c580:

    # "Rahia wasn’t very talkative and didn’t mind when I announced I’d liked to retire to my quarters."
    "Rahia war nicht sehr gesprächig und es störte sie nicht, als ich ankündigte, dass ich mich in meine Gemächer zurückziehen wolle."

# game/episode011.rpy:96
translate german episode011_fd94de38:

    # "I exchanged some pleasantries with Erylin, before politely excusing myself to my quarters."
    "Ich wechselte ein paar Höflichkeiten mit Erylin, bevor ich mich höflich entschuldigte und in meine Gemächer zurückzog."

# game/episode011.rpy:98
translate german episode011_8c0fd9c4:

    # "The podium was cleared soon after the last Acarhyn left and I was glad I could head back to the quiet of my personal quarters."
    "Das Podium wurde kurz nach dem Weggang des letzten Acarhyn abgeräumt, und ich war froh, in die Ruhe meiner Privaträume zurückkehren zu können."

# game/episode011.rpy:124
translate german ep011_feast_15c54b35:

    # "The feast had already begun when I arrived."
    "Das Fest hatte bereits begonnen, als ich ankam."

# game/episode011.rpy:125
translate german ep011_feast_8fe6404f:

    # "I spotted several familiar faces, but Eva was not in attendance, unfortunately."
    "Ich entdeckte einige bekannte Gesichter, aber Eva war leider nicht anwesend."

# game/episode011.rpy:127
translate german ep011_feast_8ac92c8a:

    # "After a hearty slap on my shoulder, a mug of ale was shoved into my general direction."
    "Nach einem kräftigen Klaps auf die Schulter wurde mir ein Krug Ale in meine Richtung geschoben."

# game/episode011.rpy:128
translate german ep011_feast_10362ade:

    # c "Uh, thanks."
    c "Äh, danke."

# game/episode011.rpy:130
translate german ep011_feast_55690065:

    # ki "Hero of the Acarhyn in da house!"
    ki "Held von Acarhyn ist im Haus!"

# game/episode011.rpy:131
translate german ep011_feast_02af7ffa:

    # c "How drunk are you already, Kit?"
    c "Wie betrunken bist du denn schon, Kit?"

# game/episode011.rpy:132
translate german ep011_feast_6850f6e1:

    # ki "I’d call this tipsy."
    ki "Ich würde das als leicht angetrunken bezeichnen."

# game/episode011.rpy:133
translate german ep011_feast_f5f7c27d:

    # c "Right."
    c "Richtig."

# game/episode011.rpy:135
translate german ep011_feast_dc1608d7:

    # ki "So, how does it feel, all of the Acarhyn swooning over you?"
    ki "Und wie fühlt es sich an, dass alle Acarhyn dich anhimmeln?"

# game/episode011.rpy:136
translate german ep011_feast_a3769a10:

    # c "They’re swooning?"
    c "Sie fallen in Ohnmacht?"

# game/episode011.rpy:137
translate german ep011_feast_20a0d17c:

    # ki "Some of them are and the rest might just be doing it in secret."
    ki "Manche tun es, die anderen vielleicht nur heimlich."

# game/episode011.rpy:139
translate german ep011_feast_e8fcf3fb:

    # "Our conversation was interrupted by several hurrahs aimed at me."
    "Unser Gespräch wurde durch mehrere Hurrarufe, die mir galten, unterbrochen."

# game/episode011.rpy:140
translate german ep011_feast_b84800d4:

    # "All Acarhyn gathered in the dining hall hefted their mugs and roared in unison."
    "Alle Acarhyn, die sich im Speisesaal versammelt hatten, hoben ihre Krüge und brüllten im Chor."

# game/episode011.rpy:142
translate german ep011_feast_5baca70b:

    # ki "See, they’re swooning."
    ki "Sieh mal, sie fallen in Ohnmacht."

# game/episode011.rpy:143
translate german ep011_feast_6ccdbb6a:

    # c "Or they’re just being polite."
    c "Oder sie sind einfach nur höflich."

# game/episode011.rpy:145
translate german ep011_feast_bc88a9d3:

    # av "Is this drunkard bothering you, [p_name]?"
    av "Stört dich dieser Betrunkene, [p_name]?"

# game/episode011.rpy:147
translate german ep011_feast_d81825a2:

    # ki "I’m just tipsy!"
    ki "Ich bin nur ein bisschen angetrunken!"

# game/episode011.rpy:149
translate german ep011_feast_e2c695fc:

    # av "Right, because everybody trips over chairs when they’re tipsy."
    av "Genau, weil ja jeder über Stühle stolpert, wenn er angetrunken ist."

# game/episode011.rpy:151
translate german ep011_feast_0bb170ed:

    # ki "It was a stool and it was very much in the way."
    ki "Es war ein Hocker und der war sehr im Weg."

# game/episode011.rpy:153
translate german ep011_feast_241d1850:

    # av "So, what’s next, [p_name_short]?"
    av "Und was kommt als Nächstes, [p_name_short]?"

# game/episode011.rpy:154
translate german ep011_feast_c9f2581c:

    # c "I can’t say too much here, but I’m working on something with Eva."
    c "Ich kann hier nicht allzu viel verraten, aber ich arbeite an etwas mit Eva."

# game/episode011.rpy:156
translate german ep011_feast_be668d2b:

    # ki "We’re getting out of here?"
    ki "Gehen wir hier weg?"

# game/episode011.rpy:157
translate german ep011_feast_20c70dc1:

    # c "That’s the plan."
    c "Das ist der Plan."

# game/episode011.rpy:158
translate german ep011_feast_c4c27cb3:

    # ki "Good, though the abundance of women is very nice, Erigone’s climate doesn’t agree with me."
    ki "Good, so schön die vielen Frauen auch sind, das Klima in Erigone bekommt mir nicht."

# game/episode011.rpy:160
translate german ep011_feast_5506dbd9:

    # av "Yeah, you’re looking decidedly pale and sweaty."
    av "Ja, du siehst ausgesprochen blass und verschwitzt aus."

# game/episode011.rpy:161
translate german ep011_feast_ec1ee7cd:

    # c "That might just be due to the amount of beer in his system."
    c "Das könnte einfach an der Menge Bier in seinem Blut liegen."

# game/episode011.rpy:163
translate german ep011_feast_abadeb84:

    # "Kit snorted at that and went in search for a couple of fresh mugs."
    "Kit schnaubte verächtlich und machte sich auf die Suche nach ein paar frischen Bechern."

# game/episode011.rpy:164
translate german ep011_feast_ba1d85c9:

    # "The rest of the feast was uneventful, save for some minor incidents where the Acarhyn fought out some petty disputes with their fists."
    "Der Rest des Festes verlief ereignislos, abgesehen von einigen kleineren Zwischenfällen, bei denen die Acarhyn einige kleinere Streitigkeiten mit den Fäusten austrugen."

# game/episode011.rpy:165
translate german ep011_feast_23beb476:

    # "In time, I started having the same problem Kit had with the furniture, almost breaking my neck over various chairs and benches on multiple occasions."
    "Mit der Zeit begann ich dasselbe Problem wie Kit mit den Möbeln zu haben und brach mir mehrmals beinahe den Hals über verschiedenen Stühlen und Bänken."

# game/episode011.rpy:166
translate german ep011_feast_582dad14:

    # "Near the end of the feast, late at night, I’d drunk enough beer to cultivate a nice hangover in the morning."
    "Gegen Ende des Festmahls, spät in der Nacht, hatte ich genug Bier getrunken, um am nächsten Morgen einen ordentlichen Kater zu haben."

# game/episode011.rpy:169
translate german ep011_feast_6a4484de:

    # "That’s when several warriors suddenly surrounded me."
    "In diesem Moment umringten mich plötzlich mehrere Krieger."

# game/episode011.rpy:172
translate german ep011_feast_fd947146:

    # "That’s when several priests suddenly surrounded me."
    "In diesem Moment umringten mich plötzlich mehrere Priester."

# game/episode011.rpy:175
translate german ep011_feast_a54b33b5:

    # "That’s when several Acarhyn suddenly surrounded me."
    "In diesem Moment umringten mich plötzlich mehrere Acarhyn."

# game/episode011.rpy:176
translate german ep011_feast_bbb8d528:

    # "Somebody pushed a tankard in my hands and made a boisterous toast, which I could only repeat phonetically."
    "Jemand drückte mir einen Bierkrug in die Hände und stieß lautstark einen Toast an, den ich nur phonetisch wiederholen konnte."

# game/episode011.rpy:177
translate german ep011_feast_a33fb2b8:

    # "Kit joined the toast, grinning from ear to ear, as if he’d gotten some very good news."
    "Kit stieß mit einem breiten Grinsen an, als hätte er eine sehr gute Nachricht erhalten."

# game/episode011.rpy:178
translate german ep011_feast_bfa6b558:

    # "The women demanded I’d tell about slaying the Undrea again, interrupting me several times for more details."
    "Die Frauen verlangten, dass ich noch einmal von der Tötung der Undrea erzähle, und unterbrachen mich mehrmals, um weitere Details zu erfahren."

# game/episode011.rpy:179
translate german ep011_feast_24b0e76d:

    # "Most of them were just as shit-faced as me and Kit, the beer flowing continuously."
    "Die meisten von ihnen waren genauso betrunken wie Kit und ich, das Bier floss unaufhörlich."

# game/episode011.rpy:180
translate german ep011_feast_084372e3:

    # "On some unseen signal, the women became relatively quiet to allow one of them to speak."
    "Auf ein unerklärliches Signal hin verstummten die Frauen, um einer von ihnen das Wort zu ermöglichen."

# game/episode011.rpy:191
translate german ep011_feast_e8a2b5f8:

    # woman "Hero of the Acarhyn, [p_name], the feast is almost at an end, so we’d like to invite you to a more private gathering."
    woman "Held von Acarhyn, [p_name], das Fest neigt sich dem Ende zu, daher möchten wir dich zu einem privateren Treffen einladen."

# game/episode011.rpy:200
translate german ep011_feast_8753efbe:

    # c "Ladies, I humbly accept!"
    c "Meine Damen, ich nehme demütig an!"

# game/episode011.rpy:207
translate german ep011_feast_615af105:

    # "I followed the women out of the dining hall."
    "Ich folgte den Frauen aus dem Speisesaal."

# game/episode011.rpy:208
translate german ep011_feast_08f53168:

    # "Kit raised his tankard when I passed him and grinned from ear to ear."
    "Kit hob seinen Bierkrug, als ich an ihm vorbeiging, und grinste über beide Ohren."

# game/episode011.rpy:218
translate german ep011_feast_029005af:

    # c "Ladies, I humbly accept, but allow me to bring my friend here."
    c "Meine Damen, ich nehme die Einladung demütig an, aber erlauben Sie mir, meine Freundin mitzubringen."

# game/episode011.rpy:219
translate german ep011_feast_920efd85:

    # woman "Of course!"
    woman "Natürlich!"

# game/episode011.rpy:226
translate german ep011_feast_25a37cf0:

    # "We followed the women out of the dining hall, Kit grinning from ear to ear."
    "Wir folgten den Frauen aus dem Speisesaal, Kit grinste über beide Ohren."

# game/episode011.rpy:232
translate german ep011_feast_eec2e101:

    # c "Ladies, I’m humbled, but I must decline."
    c "Meine Damen, ich bin beschämt, aber ich muss ablehnen."

# game/episode011.rpy:234
translate german ep011_feast_b2dad9c7:

    # "There was a murmur of disappointment and then the group of Acarhyn dissipated as quickly as they had gathered around me."
    "Ein Murmeln der Enttäuschung war zu hören, und dann löste sich die Gruppe von Acarhyn so schnell auf, wie sie sich um mich versammelt hatte."

# game/episode011.rpy:235
translate german ep011_feast_f6c1857c:

    # "Kit looked a little shocked at me, but became distracted as soon as a tankard was thrust into his hands."
    "Kit blickte mich etwas geschockt an, ließ sich aber sofort ablenken, als ihm ein Krug in die Hände gedrückt wurde."

# game/episode011.rpy:236
translate german ep011_feast_5b0b8859:

    # "The feast was nearly at an end, so I left the dining hall under boisterous cheers of the remaining Acarhyn."
    "Das Festmahl neigte sich dem Ende zu, also verließ ich den Speisesaal unter dem lauten Jubel der verbliebenen Acarhyn."

# game/episode011.rpy:241
translate german ep011_eva_vess_97be1bb1:

    # "One of the first things I did with my newfound status as Hero of the Acarhyn was a visit to the Queen."
    "Eines der ersten Dinge, die ich mit meinem neu erlangten Status als Held von Acarhyn tat, war ein Besuch bei der Königin."

# game/episode011.rpy:243
translate german ep011_eva_vess_4fcfb84f:

    # "The guards at her door let me through immediately."
    "Die Wachen an ihrer Tür ließen mich sofort durch."

# game/episode011.rpy:245
translate german ep011_eva_vess_a8bad454:

    # c "Greetings, Your Glorious and Resplendent Majesty!"
    c "Grüße, Eure glorreiche und strahlende Majestät!"

# game/episode011.rpy:247
translate german ep011_eva_vess_a098bf9f:

    # e "Haha, we greet you, oh Mighty Hero of the Acarhyn."
    e "Haha, wir grüßen dich, oh mächtiger Held von Acarhyn."

# game/episode011.rpy:249
translate german ep011_eva_vess_b4ff241a:

    # c "Hey sis!"
    c "Hey Schwester!"

# game/episode011.rpy:251
translate german ep011_eva_vess_48feb085:

    # c "Hey!"
    c "Hey!"

# game/episode011.rpy:253
translate german ep011_eva_vess_90d0fd8d:

    # e "Hey [p_name_short]."
    e "Hey [p_name_short]."

# game/episode011.rpy:254
translate german ep011_eva_vess_ab2b5b00:

    # e "I’m so glad to see you in one piece."
    e "Ich bin so froh, dich unversehrt zu sehen."

# game/episode011.rpy:255
translate german ep011_eva_vess_6fce505b:

    # c "If I knew beforehand it would be this difficult to get to see you, I might not have gone through all that trouble."
    c "Wenn ich vorher gewusst hätte, dass es so schwierig sein würde, dich zu sehen, hätte ich mir vielleicht all die Mühe nicht gemacht."

# game/episode011.rpy:256
translate german ep011_eva_vess_c40f8986:

    # e "Yes, you would."
    e "Ja, das würdest du."

# game/episode011.rpy:257
translate german ep011_eva_vess_0234cfe9:

    # c "Yeah, I totally would."
    c "Ja, das würde ich total tun."

# game/episode011.rpy:260
translate german ep011_eva_vess_93591d57:

    # e "You forged an alliance with the warrior caste, I heard?"
    e "Ich habe gehört, du hast ein Bündnis mit der Kriegerkaste geschlossen?"

# game/episode011.rpy:261
translate german ep011_eva_vess_fa0e9c11:

    # c "I did, seemed like the best choice."
    c "Ja, das schien mir die beste Wahl zu sein."

# game/episode011.rpy:262
translate german ep011_eva_vess_058e8cb6:

    # e "I can see your reasoning."
    e "Ich verstehe deine Argumentation."

# game/episode011.rpy:263
translate german ep011_eva_vess_0f025b39:

    # e "For all their faults, the warrior caste has less of a personal agenda than the priest caste does."
    e "Trotz all ihrer Fehler verfolgt die Kriegerkaste weniger persönliche Ziele als die Priesterkaste."

# game/episode011.rpy:264
translate german ep011_eva_vess_9094348a:

    # c "So I reckoned."
    c "Das habe ich mir auch gedacht."

# game/episode011.rpy:266
translate german ep011_eva_vess_69f047ac:

    # e "You forged an alliance with the priesthood, I heard?"
    e "Ich habe gehört, du hättest ein Bündnis mit der Priesterschaft geschlossen?"

# game/episode011.rpy:267
translate german ep011_eva_vess_e0b0307c:

    # c "Yes, they seemed like the strongest choice."
    c "Ja, sie schienen die beste Wahl zu sein."

# game/episode011.rpy:268
translate german ep011_eva_vess_3c0ebf61:

    # e "They likely are."
    e "Das sind sie wahrscheinlich."

# game/episode011.rpy:269
translate german ep011_eva_vess_3cb5f49c:

    # e "Be careful though, Erylin is very ambitious."
    e "Aber Sei vorsichtig, Erylin ist sehr ehrgeizig."

# game/episode011.rpy:270
translate german ep011_eva_vess_d6515187:

    # c "I know, several people already warned me."
    c "Ich weiß, mehrere Leute haben mich schon gewarnt."

# game/episode011.rpy:271
translate german ep011_eva_vess_1436c57b:

    # c "But you know, keep your enemies closer and all that."
    c "Aber du weißt ja, man soll seine Feinde immer nah bei sich haben und so weiter."

# game/episode011.rpy:273
translate german ep011_eva_vess_a2f25173:

    # e "And you were victorious in the Hunt all by yourself, I heard?"
    e "Und du hast die Jagd ganz allein gewonnen, habe ich gehört?"

# game/episode011.rpy:274
translate german ep011_eva_vess_a2364198:

    # c "Yes, it seemed like a good decision not to get mired in Acarhyn politics."
    c "Ja, es schien eine gute Entscheidung zu sein, sich nicht in die Politik von Acarhyn verwickeln zu lassen."

# game/episode011.rpy:275
translate german ep011_eva_vess_302494cf:

    # e "A wise decision."
    e "Eine weise Entscheidung."

# game/episode011.rpy:276
translate german ep011_eva_vess_1e4ec4f0:

    # c "Let Rahia and Erylin fight for control without me as a bargaining chip."
    c "Lass Rahia und Erylin um die Macht kämpfen, ohne dass ich als Druckmittel diene."

# game/episode011.rpy:277
translate german ep011_eva_vess_4fef3055:

    # e "Exactly."
    e "Genau."

# game/episode011.rpy:279
translate german ep011_eva_vess_f72a7b15:

    # e "So, what will you do now?"
    e "Und was wirst du jetzt tun?"

# game/episode011.rpy:280
translate german ep011_eva_vess_74442150:

    # c "I was hoping you could grant me a mansion and titles, so I can live my twilight years in peace here."
    c "Ich hatte gehofft, du könntest mir ein Herrenhaus und Titel gewähren, damit ich hier meinen Lebensabend in Frieden verbringen kann."

# game/episode011.rpy:282
translate german ep011_eva_vess_75e0d37e:

    # e "[p_name_short]..."
    e "[p_name_short]..."

# game/episode011.rpy:283
translate german ep011_eva_vess_9f85b645:

    # c "Yeah."
    c "Ja."

# game/episode011.rpy:284
translate german ep011_eva_vess_226d2b7d:

    # c "I want to get you off this rock."
    c "Ich will dich von diesem Felsen runterholen."

# game/episode011.rpy:286
translate german ep011_eva_vess_eb6e1fc8:

    # e "On that ship you arrived with?"
    e "Mit welchem ​​Schiff bist du angekommen?"

# game/episode011.rpy:287
translate german ep011_eva_vess_bfc1c230:

    # c "Preferably."
    c "Vorzugsweise."

# game/episode011.rpy:288
translate german ep011_eva_vess_b1926d50:

    # e "According to my reports, the repairs should be nearly finished."
    e "Meinen Berichten zufolge sollten die Reparaturen so gut wie abgeschlossen sein."

# game/episode011.rpy:289
translate german ep011_eva_vess_f1398536:

    # c "In that case, pack your bags!"
    c "Dann pack deine Koffer!"

# game/episode011.rpy:291
translate german ep011_eva_vess_ad273ec7:

    # e "Well, there’s this whole problem of me being Queen and all that."
    e "Nun ja, da ist dieses ganze Problem, dass ich Königin bin und so weiter."

# game/episode011.rpy:292
translate german ep011_eva_vess_497a4ad7:

    # c "Right, it’s the minor details that always throw a spanner in the works."
    c "Genau, es sind die kleinen Details, die immer wieder Probleme bereiten."

# game/episode011.rpy:293
translate german ep011_eva_vess_e029a855:

    # c "Nobody else wants the job?"
    c "Will denn sonst niemand den Job?"

# game/episode011.rpy:295
translate german ep011_eva_vess_99e9ccfd:

    # e "I’m sure many do, but I’m the Virgin Queen."
    e "Ich bin sicher, viele tun das, aber ich bin die Jungfräuliche Königin."

# game/episode011.rpy:296
translate german ep011_eva_vess_441f791e:

    # c "You do want to go, don’t you?"
    c "Du willst doch mitgehen, oder?"

# game/episode011.rpy:297
translate german ep011_eva_vess_86459b9d:

    # c "Or have you come to love your subjects and feel duty-bound to lead them?"
    c "Oder haben Sie Ihre Untertanen lieben gelernt und fühlen sich verpflichtet, sie zu führen?"

# game/episode011.rpy:299
translate german ep011_eva_vess_a65955cc:

    # e "Lord no."
    e "Lord nein."

# game/episode011.rpy:301
translate german ep011_eva_vess_356e4b5c:

    # e "I wouldn’t wish this title on anyone and I’m sure the Acarhyn can play their political games without me being their figurehead."
    e "Ich würde diesen Titel niemandem wünschen und ich bin mir sicher, dass die Acarhyn ihre politischen Spielchen auch ohne mich als Galionsfigur spielen können."

# game/episode011.rpy:302
translate german ep011_eva_vess_7bf38bf4:

    # e "They’re going to pursue us mercilessly after we escape."
    e "Sie werden uns nach unserer Flucht gnadenlos verfolgen."

# game/episode011.rpy:303
translate german ep011_eva_vess_743fd5d4:

    # e "So I want you to make sure everyone is behind this attempt to rescue me."
    e "Deshalb möchte ich, dass du sicherstellst, dass alle diesen Rettungsversuch für mich unterstützen."

# game/episode011.rpy:304
translate german ep011_eva_vess_083c9d72:

    # c "I think everybody is behind this, but I’ll ask them to be sure."
    c "Ich glaube, alle stehen dahinter, aber ich werde sie zur Sicherheit noch einmal fragen."

# game/episode011.rpy:305
translate german ep011_eva_vess_1c76a7b1:

    # c "So there’s little chance of them letting you go voluntarily?"
    c "Also stehen die Chancen schlecht, dass sie dich freiwillig gehen lassen?"

# game/episode011.rpy:307
translate german ep011_eva_vess_34524ce4:

    # e "No chance, I’d say."
    e "Keine Chance, würde ich sagen."

# game/episode011.rpy:308
translate german ep011_eva_vess_b31c4e00:

    # c "It would be out of character for you to demand a royal interplanetary mission, I take it?"
    c "Es wäre untypisch für dich, eine königliche interplanetare Mission zu fordern, nehme ich an?"

# game/episode011.rpy:310
translate german ep011_eva_vess_a702e55f:

    # e "Considering the location of Erigone is one of the best kept secrets in the known universe, I’d say we shouldn’t count on a lot of support."
    e "Wenn man bedenkt, dass der Standort von Erigone eines der bestgehüteten Geheimnisse im bekannten Universum ist, würde ich sagen, dass wir nicht mit viel Unterstützung rechnen sollten."

# game/episode011.rpy:311
translate german ep011_eva_vess_75e969d9:

    # c "Abducting you while you’re in the Citadel is going to be a challenge."
    c "Dich in der Zitadelle zu entführen, wird eine Herausforderung sein."

# game/episode011.rpy:312
translate german ep011_eva_vess_bd1661fc:

    # e "I know."
    e "Ich weiß."

# game/episode011.rpy:314
translate german ep011_eva_vess_edb4b3c1:

    # e "What I could demand is a tour of Erigone."
    e "Was ich verlangen könnte, wäre eine Tour durch Erigone."

# game/episode011.rpy:315
translate german ep011_eva_vess_c1133222:

    # e "It’s rare for the Acarhyn monarch to inspect her lands, but I have the right to do so."
    e "Es ist selten, dass die Monarchin von Acarhyn ihre Ländereien inspiziert, aber ich habe das Recht dazu."

# game/episode011.rpy:316
translate german ep011_eva_vess_70f08622:

    # c "So you let us know your route through the Acarhyn heartlands and we’ll be waiting for you in some backwater village?"
    c "Also teile uns deine Route durch das Herzland von Acarhyn mit und wir erwarten dich in einem abgelegenen Dorf?"

# game/episode011.rpy:318
translate german ep011_eva_vess_6b73b6cd:

    # e "That sounds like a plan."
    e "Das klingt nach einem Plan."

# game/episode011.rpy:319
translate german ep011_eva_vess_ef9c4557:

    # e "I’ll make sure my security is light that day."
    e "Ich werde dafür sorgen, dass meine Sicherheitsvorkehrungen an diesem Tag leicht sind."

# game/episode011.rpy:321
translate german ep011_eva_vess_b7ac18f4:

    # e "Problem is that you’ll be prime suspects almost immediately."
    e "Das Problem ist, dass ihr fast sofort unter Verdacht stehen werdet."

# game/episode011.rpy:322
translate german ep011_eva_vess_8235c1c5:

    # e "So this needs to happen near the Aegisthus Shipyards."
    e "Das muss also in der Nähe der Aegisthos-Werft stattfinden."

# game/episode011.rpy:323
translate german ep011_eva_vess_18740c44:

    # e "Hopefully there isn’t enough time to alert any of the authorities."
    e "Hoffentlich bleibt nicht mehr genug Zeit, um die Behörden zu alarmieren."

# game/episode011.rpy:324
translate german ep011_eva_vess_779e097d:

    # c "Will we be able to leave those shipyards?"
    c "Werden wir diese Werften verlassen können?"

# game/episode011.rpy:326
translate german ep011_eva_vess_57826a59:

    # e "Yes, I could sign the necessary paperwork to release your ship."
    e "Ja, ich könnte die notwendigen Unterlagen zur Freigabe Ihres Schiffes unterzeichnen."

# game/episode011.rpy:327
translate german ep011_eva_vess_cb8053a8:

    # e "If they don’t look too closely it should be fine."
    e "Solange sie nicht zu genau hinschauen, sollte alles in Ordnung sein."

# game/episode011.rpy:328
translate german ep011_eva_vess_e8ef5c8d:

    # c "Good, in that case, let me kn-"
    c "Gut, in diesem Fall lass es mich wissen."

# game/episode011.rpy:333
translate german ep011_eva_vess_90039e61:

    # "Several Acarhyn burst inside Eva’s quarters."
    "Mehrere Acarhyn stürmten in Evas Quartier."

# game/episode011.rpy:334
translate german ep011_eva_vess_2bb605ca:

    # "At first I thought they were coming for me, but their leader didn’t even acknowledge my presence."
    "Zuerst dachte ich, sie kämen wegen mir, aber ihr Anführer beachtete mich gar nicht."

# game/episode011.rpy:338
translate german ep011_eva_vess_46c87ed3:

    # woman "Your Majesty, a situation has arisen that requires your attention."
    woman "Eure Majestät, es hat sich eine Situation ergeben, die Eure Aufmerksamkeit erfordert."

# game/episode011.rpy:340
translate german ep011_eva_vess_fc7aba09:

    # e "What’s the matter, soldier?"
    e "Was ist los, Soldat?"

# game/episode011.rpy:342
translate german ep011_eva_vess_b0b13bdb:

    # woman "We’ve apprehended one of the guests near our communication relays."
    woman "Wir haben einen der Gäste in der Nähe unserer Kommunikationsrelais festgenommen."

# game/episode011.rpy:343
translate german ep011_eva_vess_13edd0a6:

    # "My stomach turned at hearing the news."
    "Mir wurde ganz flau im Magen, als ich die Nachricht hörte."

# game/episode011.rpy:345
translate german ep011_eva_vess_17d084e2:

    # "Eva had already gone behind the privacy screen to change into her robes."
    "Eva war bereits hinter den Sichtschutz gegangen, um sich in ihren Bademantel umzuziehen."

# game/episode011.rpy:347
translate german ep011_eva_vess_b703e13e:

    # "When she emerged, she gestured me to come with her, following the Acarhyn warrior."
    "Als sie auftauchte, bedeutete sie mir, ihr zu folgen, und zwar dem Krieger von Acarhyn."

# game/episode011.rpy:349
translate german ep011_eva_vess_feab0d4a:

    # "She led us through the throne room, where a few of the Acarhyn had already gathered."
    "Sie führte uns durch den Thronsaal, wo sich bereits einige der Acarhyn versammelt hatten."

# game/episode011.rpy:351
translate german ep011_eva_vess_4f442ff6:

    # "Eva sat down on her throne and regarded everyone coolly."
    "Eva setzte sich auf ihren Thron und betrachtete alle mit kühler Miene."

# game/episode011.rpy:353
translate german ep011_eva_vess_6927576c:

    # e "What is the matter?"
    e "Was ist los?"

# game/episode011.rpy:355
translate german ep011_eva_vess_30144156:

    # er "We’ve captured a traitor in our midst, Your Majesty."
    er "Majestät, wir haben einen Verräter in unseren Reihen gefasst."

# game/episode011.rpy:356
translate german ep011_eva_vess_8d136e23:

    # er "She was found snooping around the communication towers of the Citadel."
    er "Sie wurde dabei erwischt, wie sie in der Nähe der Kommunikationstürme der Zitadelle herumschnüffelte."

# game/episode011.rpy:357
translate german ep011_eva_vess_484075d1:

    # er "According to our experts, one message was sent to an unknown receiver."
    er "Unseren Experten zufolge wurde eine Nachricht an einen unbekannten Empfänger gesendet."

# game/episode011.rpy:359
translate german ep011_eva_vess_c5da750f:

    # e "Bring her before me."
    e "Bring sie vor mich."

# game/episode011.rpy:361
translate german ep011_eva_vess_a0c995c0:

    # er "At once, Your Majesty."
    er "Sofort, Eure Majestät."

# game/episode011.rpy:363
translate german ep011_eva_vess_5d01d922:

    # "We all waited with bated breath until two Acarhyn entered the throne room, dragging a girl between them."
    "Wir alle hielten den Atem an, bis zwei Acarhyn den Thronsaal betraten und ein Mädchen zwischen sich herzogen."

# game/episode011.rpy:365
translate german ep011_eva_vess_624858ad:

    # "Vess."
    "Vess."

# game/episode011.rpy:367
translate german ep011_eva_vess_e4fadbfd:

    # "She looked busted up and utterly defeated."
    "Sie sah völlig fertig und am Ende ihrer Kräfte aus."

# game/episode011.rpy:369
translate german ep011_eva_vess_87b90d8b:

    # "When the Acarhyn dropped her before the throne she didn’t even bother to stand up."
    "Als der Acarhyn sie vor dem Thron absetzte, machte sie sich nicht einmal die Mühe, aufzustehen."

# game/episode011.rpy:371
translate german ep011_eva_vess_eda8f4c0:

    # er "Stand in the presence of Her Majesty!"
    er "Tritt vor Ihre Majestät!"

# game/episode011.rpy:373
translate german ep011_eva_vess_5ac9d6f3:

    # "With some effort Vess got to her feet."
    "Mit einiger Mühe rappelte sich Vess auf."

# game/episode011.rpy:374
translate german ep011_eva_vess_86b3d689:

    # e "What were you doing at those communication towers?"
    e "Was hast du an diesen Sendetürmen gemacht?"

# game/episode011.rpy:375
translate german ep011_eva_vess_7b726467:

    # "At first I didn’t think Vess would speak, but when she did her voice was soft, yet clear."
    "Zuerst dachte ich nicht, dass Vess sprechen würde, aber als sie es tat, war ihre Stimme sanft, aber dennoch deutlich."

# game/episode011.rpy:377
translate german ep011_eva_vess_5746f781:

    # ve "I sent a message."
    ve "Ich habe eine Nachricht geschickt."

# game/episode011.rpy:379
translate german ep011_eva_vess_123bb436:

    # er "You’ll address the Queen with her proper title."
    er "Du wirst die Königin mit ihrem korrekten Titel ansprechen."

# game/episode011.rpy:381
translate german ep011_eva_vess_fa8aacf6:

    # e "That isn’t necessary, it will only make these proceedings more tedious."
    e "Das ist nicht nötig, es würde das Verfahren nur unnötig in die Länge ziehen."

# game/episode011.rpy:382
translate german ep011_eva_vess_e4289843:

    # e "So you sent a message, that much has already been confirmed."
    e "Du hast also eine Nachricht geschickt, das ist bereits bestätigt."

# game/episode011.rpy:383
translate german ep011_eva_vess_7a288a4f:

    # e "To whom did you send the message?"
    e "An wen hast du die Nachricht geschickt?"

# game/episode011.rpy:385
translate german ep011_eva_vess_3240cb7e:

    # ve "My master."
    ve "Mein Meister."

# game/episode011.rpy:387
translate german ep011_eva_vess_97422588:

    # e "You have a master?"
    e "Du hast einen Meister?"

# game/episode011.rpy:388
translate german ep011_eva_vess_d3af4f45:

    # e "I thought you were part of the crew of the Iron Bastard?"
    e "Ich dachte, du wärst Teil der Besatzung der Iron Bastard?"

# game/episode011.rpy:390
translate german ep011_eva_vess_77c75faa:

    # ve "I-"
    ve "Ich-"

# game/episode011.rpy:392
translate german ep011_eva_vess_e13d35fd:

    # "Her eyes darted to me for the first time and the look she gave me was one of profound sadness, as if she were begging me for forgiveness with just her eyes."
    "Zum ersten Mal huschte ihr Blick zu mir, und der Blick, den sie mir zuwarf, war von tiefer Traurigkeit geprägt, als ob sie mich allein mit ihren Augen um Vergebung bitten wollte."

# game/episode011.rpy:394
translate german ep011_eva_vess_398f08cb:

    # er "Answer the Virgin Queen at once!"
    er "Antworte der Jungfräulichen Königin sofort!"

# game/episode011.rpy:396
translate german ep011_eva_vess_0b03bcf8:

    # ve "I was part of the crew of the Iron Bastard, but I also serve another."
    ve "Ich war Teil der Besatzung der Iron Bastard, aber ich diene auch einer anderen."

# game/episode011.rpy:398
translate german ep011_eva_vess_5e8390ba:

    # e "Who is this master you speak of?"
    e "Wer ist dieser Meister, von dem du sprichst?"

# game/episode011.rpy:400
translate german ep011_eva_vess_8b055440:

    # ve "I only know him as the Confessor."
    ve "Ich kenne ihn nur als den Beichtvater."

# game/episode011.rpy:401
translate german ep011_eva_vess_82bea290:

    # "Eva looked at Erylin, but the priestess shook her head, neither had heard of the name before."
    "Eva blickte Erylin an, doch die Priesterin schüttelte den Kopf; keine von beiden hatte je zuvor von diesem Namen gehört."

# game/episode011.rpy:403
translate german ep011_eva_vess_18ebbb22:

    # e "What was the intent of your message?"
    e "Was war die Absicht deiner Nachricht?"

# game/episode011.rpy:405
translate german ep011_eva_vess_40c466ef:

    # ve "To let my master know where we are, and that I’ve found my target."
    ve "Um meinem Herrn mitzuteilen, wo wir uns befinden und dass ich mein Ziel gefunden habe."

# game/episode011.rpy:407
translate german ep011_eva_vess_9d9a5c6c:

    # e "Your target?{w} Who?"
    e "Ihr Ziel?{w} Wer?"

# game/episode011.rpy:409
translate german ep011_eva_vess_32de5828:

    # ve "You."
    ve "Du."

# game/episode011.rpy:411
translate german ep011_eva_vess_c0dc88fc:

    # "That last statement caused a general uproar, which had to be calmed by several guards in the throne room."
    "Diese letzte Aussage löste einen allgemeinen Aufruhr aus, der von mehreren Wachen im Thronsaal beruhigt werden musste."

# game/episode011.rpy:413
translate german ep011_eva_vess_fe9de038:

    # e "The Confessor is looking for me?"
    e "Der Beichtvater sucht mich?"

# game/episode011.rpy:414
translate german ep011_eva_vess_55fadba1:

    # e "What does he want from me?"
    e "Was will er von mir?"

# game/episode011.rpy:416
translate german ep011_eva_vess_ec2a854c:

    # ve "I don’t know."
    ve "Ich weiß es nicht."

# game/episode011.rpy:418
translate german ep011_eva_vess_13cf3469:

    # er "Lies!{w} You’re tasked with finding the Virgin Queen without knowing to what purpose?!"
    er "Lügen!{w} Du hast die Aufgabe, die Jungfräuliche Königin zu finden, ohne zu wissen, zu welchem ​​Zweck?!"

# game/episode011.rpy:420
translate german ep011_eva_vess_316810b0:

    # ve "Yes, that’s correct."
    ve "Ja, das ist richtig."

# game/episode011.rpy:422
translate german ep011_eva_vess_bb2f0258:

    # er "Nonsense, you’re holding something back."
    er "Unsinn, du verschweigst etwas."

# game/episode011.rpy:424
translate german ep011_eva_vess_31e36c51:

    # e "Erylin!{w} I am conducting the questioning here."
    e "Erylin!{w} Ich führe hier die Befragung durch."

# game/episode011.rpy:426
translate german ep011_eva_vess_1573f402:

    # "The priestess was taken aback by Eva’s strong response, which caused a shocked murmur in the throne room."
    "Die Priesterin war von Evas heftiger Reaktion überrascht, was im Thronsaal ein überraschtes Raunen auslöste."

# game/episode011.rpy:427
translate german ep011_eva_vess_a42595ca:

    # er "Of course, Your Majesty."
    er "Selbstverständlich, Eure Majestät."

# game/episode011.rpy:429
translate german ep011_eva_vess_04dfbb16:

    # e "So the Confessor will land on Erigone if our defence systems don’t take care of his ship?"
    e "Wird der Beichtvater also auf Erigone landen, wenn unsere Verteidigungssysteme sein Schiff nicht abfangen?"

# game/episode011.rpy:431
translate german ep011_eva_vess_554ba6d7:

    # ve "Your defences will be quite useless against the Confessor’s ship."
    ve "Deine Verteidigung wird gegen das Schiff des Bekenners völlig nutzlos sein."

# game/episode011.rpy:433
translate german ep011_eva_vess_3ffdaee0:

    # e "Is that so?"
    e "Ist das so?"

# game/episode011.rpy:434
translate german ep011_eva_vess_8b2e8b2c:

    # e "Do you know his intended landing site?"
    e "Kennst du seinen geplanten Landeplatz?"

# game/episode011.rpy:436
translate german ep011_eva_vess_2fd74067:

    # ve "I do."
    ve "Ja."

# game/episode011.rpy:437
translate german ep011_eva_vess_840fa693:

    # e "You’ll share that location with us."
    e "Du wirst uns diesen Standort mitteilen."

# game/episode011.rpy:438
translate german ep011_eva_vess_0d0e0431:

    # ve "I will."
    ve "Das werde ich."

# game/episode011.rpy:440
translate german ep011_eva_vess_5fb1d94d:

    # e "How soon will his vessel land?"
    e "Wie lange dauert es noch, bis sein Schiff anlegt?"

# game/episode011.rpy:442
translate german ep011_eva_vess_a5e7b7ef:

    # ve "In a few hours at most."
    ve "In höchstens ein paar Stunden."

# game/episode011.rpy:444
translate german ep011_eva_vess_664592b1:

    # e "This has given me a lot to think on."
    e "Das hat mir viel Stoff zum Nachdenken gegeben."

# game/episode011.rpy:445
translate german ep011_eva_vess_77a54bb0:

    # e "Take this girl away to the dungeon, we’ll speak with her again at a later time."
    e "Bring dieses Mädchen in den Kerker, wir sprechen später noch einmal mit ihr."

# game/episode011.rpy:447
translate german ep011_eva_vess_f3aed3ab:

    # "Vess was forced out of the throne room by her two guards."
    "Vess wurde von ihren beiden Wachen aus dem Thronsaal gedrängt."

# game/episode011.rpy:449
translate german ep011_eva_vess_e516d8c9:

    # "As she exited, the girl cast one forlorn glance at me over her shoulder."
    "Als sie hinausging, warf das Mädchen mir einen traurigen Blick über die Schulter zu."

# game/episode011.rpy:451
translate german ep011_eva_vess_c8693783:

    # "The next few hours were abuzz with activity."
    "Die nächsten Stunden waren von regem Treiben geprägt."

# game/episode011.rpy:453
translate german ep011_eva_vess_b1edb967:

    # "A force of Acarhyn warriors and priestesses was assembled in the Citadel’s courtyard."
    "Im Hof ​​der Zitadelle hatte sich eine Streitmacht aus acarhynischen Kriegern und Priesterinnen versammelt."

# game/episode011.rpy:454
translate german ep011_eva_vess_ea8130b5:

    # "Erylin would lead them personally to the Confessor’s landing site."
    "Erylin würde sie persönlich zum Landeplatz des Bekenners führen."

# game/episode011.rpy:455
translate german ep011_eva_vess_423563f5:

    # "The idea was apparently to apprehend the Confessor and take him back to the Citadel."
    "Die Idee war offenbar, den Bekenner gefangen zu nehmen und ihn zurück zur Zitadelle zu bringen."

# game/episode011.rpy:457
translate german ep011_eva_vess_25818eeb:

    # "The whole situation seriously messed up the plans I’d made with Eva before."
    "Die ganze Situation hat meine Pläne, die ich zuvor mit Eva geschmiedet hatte, völlig durcheinandergebracht."

# game/episode011.rpy:458
translate german ep011_eva_vess_443f7357:

    # "The upside was that Erylin would leave the Citadel."
    "Der Vorteil war, dass Erylin die Zitadelle verlassen würde."

# game/episode011.rpy:459
translate german ep011_eva_vess_5f5a3c5d:

    # "By now everyone of the crew had heard about what transpired in the throne room."
    "Inzwischen hatte jeder in der Mannschaft von dem gehört, was sich im Thronsaal zugetragen hatte."

# game/episode011.rpy:461
translate german ep011_eva_vess_4786aad6:

    # "I locked myself in my quarters for a while, not willing to deal with any questions about Vess’ betrayal."
    "Ich schloss mich eine Weile in meinen Gemächern ein, da ich mich nicht mit Fragen zu Vess\' Verrat auseinandersetzen wollte."

# game/episode011.rpy:462
translate german ep011_eva_vess_7cab4583:

    # "Only once I was disturbed, when one of Eva’s personal guards delivered an invitation."
    "Nur einmal wurde ich gestört, als einer von Evas Leibwächtern mir eine Einladung überbrachte."

# game/episode011.rpy:468
translate german ep011_eva_talk_ff0f438a:

    # "Eva had asked us to come to her chambers to talk strategy."
    "Eva hatte uns gebeten, in ihre Gemächer zu kommen, um über die Strategie zu sprechen."

# game/episode011.rpy:469
translate german ep011_eva_talk_11e5baf6:

    # "The guards again let me through without issue and I was surprised to be the first one to arrive."
    "Die Wachen ließen mich wieder ohne Probleme durch und ich war überrascht, als Erster anzukommen."

# game/episode011.rpy:471
translate german ep011_eva_talk_0dd52b91:

    # e "Are you alright, [p_name_short]?"
    e "Alles in Ordnung bei dir, [p_name_short]?"

# game/episode011.rpy:472
translate german ep011_eva_talk_5050fc38:

    # c "Still reeling about the news, to be honest."
    c "Ich bin ehrlich gesagt immer noch fassungslos über die Nachricht."

# game/episode011.rpy:473
translate german ep011_eva_talk_036464ec:

    # c "We knew someone had been sending messages before, but never found out who did it."
    c "Wir wussten, dass schon vorher jemand Nachrichten verschickt hatte, haben aber nie herausgefunden, wer es war."

# game/episode011.rpy:475
translate german ep011_eva_talk_0f01bdc0:

    # e "Did you suspect the girl?"
    e "Hattest du Verdacht gegen das Mädchen?"

# game/episode011.rpy:476
translate german ep011_eva_talk_24321612:

    # c "Looking back at it all, she’d be one of the prime suspects."
    c "Wenn man alles rückblickend betrachtet, wäre sie eine der Hauptverdächtigen."

# game/episode011.rpy:477
translate german ep011_eva_talk_031567f5:

    # c "I’d really like to talk to her at some point."
    c "Ich würde mich irgendwann wirklich gerne mit ihr unterhalten."

# game/episode011.rpy:479
translate german ep011_eva_talk_3680e25a:

    # e "You have permission to go into the dungeons and visit her."
    e "Du hast die Erlaubnis, in die Verliese zu gehen und sie zu besuchen."

# game/episode011.rpy:480
translate german ep011_eva_talk_e897fbb0:

    # c "Thank you."
    c "Danke."

# game/episode011.rpy:482
translate german ep011_eva_talk_055d1cc5:

    # "The door opened and Lilly stormed inside."
    "Die Tür öffnete sich und Lilly stürmte hinein."

# game/episode011.rpy:483
translate german ep011_eva_talk_379272a9:

    # l "Eva!"
    l "Eva!"

# game/episode011.rpy:486
translate german ep011_eva_talk_8ee04107:

    # "Eva was too surprised to utter anything, but accepted her sister’s wild embrace wholeheartedly."
    "Eva war zu überrascht, um etwas zu sagen, nahm die stürmische Umarmung ihrer Schwester aber von ganzem Herzen an."

# game/episode011.rpy:488
translate german ep011_eva_talk_ba867170:

    # "Eva was too surprised to utter anything, but accepted her friend’s wild embrace wholeheartedly."
    "Eva war zu überrascht, um etwas zu sagen, nahm die stürmische Umarmung ihrer Freundin aber von ganzem Herzen an."

# game/episode011.rpy:489
translate german ep011_eva_talk_fa1e29b0:

    # e "I’ve missed you so much."
    e "Ich habe dich so sehr vermisst."

# game/episode011.rpy:490
translate german ep011_eva_talk_6b51495e:

    # l "Me too."
    l "Ich auch."

# game/episode011.rpy:492
translate german ep011_eva_talk_af9d08c1:

    # l "Once everyone is gone I’m going to talk your ears off about everything we went through to get here."
    l "Sobald alle weg sind, werde ich dir die Ohren vollquatschen mit allem, was wir durchgemacht haben, um hierher zu kommen."

# game/episode011.rpy:493
translate german ep011_eva_talk_56dbfe69:

    # e "Looking forward to it already."
    e "Ich freue mich schon sehr darauf."

# game/episode011.rpy:495
translate german ep011_eva_talk_812b0f0d:

    # e "You brought Aunt Nadya and Aven too?"
    e "Hast du Tante Nadya und Aven auch mitgebracht?"

# game/episode011.rpy:497
translate german ep011_eva_talk_5460e3ab:

    # e "You brought Nadya and Aven too?"
    e "Hast du Nadya und Aven auch mitgebracht?"

# game/episode011.rpy:498
translate german ep011_eva_talk_a8858f90:

    # l "I did."
    l "Das habe ich."

# game/episode011.rpy:500
translate german ep011_eva_talk_e7ccd422:

    # av "Will you be very cross if we don’t call you Your Majesty?"
    av "Wärst du sehr verärgert, wenn wir dich nicht „Eure Majestät“ nennen?"

# game/episode011.rpy:501
translate german ep011_eva_talk_72330704:

    # e "I’ll allow it."
    e "Ich erlaube es."

# game/episode011.rpy:502
translate german ep011_eva_talk_a575249a:

    # na "It’s so good seeing you again."
    na "Es ist so schön, dich wiederzusehen."

# game/episode011.rpy:503
translate german ep011_eva_talk_a2572a5e:

    # e "It’s been ages, but I’m so glad you all agreed to rescue me."
    e "Es ist schon ewig her, aber ich bin so froh, dass ihr alle zugestimmt habt, mich zu retten."

# game/episode011.rpy:506
translate german ep011_eva_talk_640dbc11:

    # "Nadya took her niece into a warm embrace and the two remained motionless for a long while."
    "Nadya nahm ihre Nichte in eine herzliche Umarmung, und die beiden blieben lange Zeit regungslos."

# game/episode011.rpy:508
translate german ep011_eva_talk_5d2daec8:

    # "Nadya took Eva into a warm embrace and the two remained motionless for a long while."
    "Nadya nahm Eva in eine herzliche Umarmung und die beiden verharrten lange Zeit regungslos."

# game/episode011.rpy:509
translate german ep011_eva_talk_da1a3136:

    # "We all made ourselves comfortable in the Queen’s Chambers and started drawing up a battle plan."
    "Wir machten es uns alle in den Gemächern der Königin bequem und begannen, einen Schlachtplan auszuarbeiten."

# game/episode011.rpy:511
translate german ep011_eva_talk_1291d6b6:

    # c "The plan was originally for Eva to go on some grand tour through the countryside."
    c "Ursprünglich war geplant, dass Eva eine große Rundreise durch die Landschaft unternimmt."

# game/episode011.rpy:512
translate german ep011_eva_talk_aa451027:

    # c "We’d abduct her in a village near the shipyards and escape on the Bastard."
    c "Wir würden sie in einem Dorf in der Nähe der Werften entführen und mit der Bastard fliehen."

# game/episode011.rpy:513
translate german ep011_eva_talk_b4575789:

    # c "Vess’ little betrayal and the coming of the Confessor has just made that plan redundant."
    c "Vess\' kleiner Verrat und das Erscheinen des Bekenners haben diesen Plan hinfällig gemacht."

# game/episode011.rpy:515
translate german ep011_eva_talk_ae0e2de8:

    # na "So what do we do now?"
    na "Was machen wir jetzt?"

# game/episode011.rpy:517
translate german ep011_eva_talk_501ad0e2:

    # e "This situation could work to our advantage."
    e "Diese Situation könnte sich für uns als Vorteil erweisen."

# game/episode011.rpy:519
translate german ep011_eva_talk_70f4439b:

    # na "How so?"
    na "Wieso?"

# game/episode011.rpy:521
translate german ep011_eva_talk_38dd709e:

    # e "Erylin is going to use this situation for her own personal gain."
    e "Erylin wird diese Situation zu ihrem eigenen Vorteil ausnutzen."

# game/episode011.rpy:522
translate german ep011_eva_talk_28f3b1e5:

    # e "That Confessor is after me, so she’ll probably try to leverage this in some way."
    e "Die Beichtmutter ist hinter mir her, also wird sie das wahrscheinlich irgendwie ausnutzen wollen."

# game/episode011.rpy:524
translate german ep011_eva_talk_d2771a5c:

    # av "By allying herself with the Confessor?"
    av "Indem sie sich mit dem Bekenner verbündet?"

# game/episode011.rpy:526
translate german ep011_eva_talk_b04e1240:

    # e "Possibly."
    e "Möglicherweise."

# game/episode011.rpy:527
translate german ep011_eva_talk_c4dbfb79:

    # c "Likely even, if she fails to apprehend him."
    c "Wahrscheinlich sogar, wenn es ihr nicht gelingt, ihn zu fassen."

# game/episode011.rpy:528
translate german ep011_eva_talk_3c5e1d6a:

    # e "So, if I’m still here once Erylin gets back from the jungle, I could be in danger."
    e "Also, wenn ich noch hier bin, wenn Erylin aus dem Dschungel zurückkommt, könnte ich in Gefahr sein."

# game/episode011.rpy:529
translate german ep011_eva_talk_3191c233:

    # e "If we could convince the warrior caste of this, they might even aid us in escaping."
    e "Wenn wir die Kriegerkaste davon überzeugen könnten, würden sie uns vielleicht sogar bei der Flucht helfen."

# game/episode011.rpy:531
translate german ep011_eva_talk_c0dcdee5:

    # av "They’d support a Queen in exile?"
    av "Würden sie eine Königin im Exil unterstützen?"

# game/episode011.rpy:533
translate german ep011_eva_talk_552de0cd:

    # e "They might, if we are convincing enough."
    e "Das könnten sie, wenn wir überzeugend genug sind."

# game/episode011.rpy:535
translate german ep011_eva_talk_a44a6e7a:

    # e "Could you try to talk with Rahia, [p_name]?"
    e "Könntest du versuchen, mit Rahia zu sprechen, [p_name]?"

# game/episode011.rpy:537
translate german ep011_eva_talk_d0dd6ebf:

    # e "I’ll try and talk to Rahia about this."
    e "Ich werde versuchen, mit Rahia darüber zu sprechen."

# game/episode011.rpy:539
translate german ep011_eva_talk_e49a1e6e:

    # l "Is our ship capable of flying again?"
    l "Ist unser Schiff wieder flugfähig?"

# game/episode011.rpy:541
translate german ep011_eva_talk_14454509:

    # e "I believe it is, they’re working on putting the finishing touches on the interior, I believe."
    e "Ich glaube schon, sie arbeiten gerade an den letzten Details des Innenausbaus, glaube ich."

# game/episode011.rpy:543
translate german ep011_eva_talk_e8bee627:

    # l "How will we reach the shipyards?"
    l "Wie gelangen wir zu den Werften?"

# game/episode011.rpy:545
translate german ep011_eva_talk_2d839eb5:

    # e "The easiest way would be to use one of the armored vehicles you arrived in."
    e "Am einfachsten wäre es, eines der gepanzerten Fahrzeuge zu benutzen, mit denen Sie angekommen sind."

# game/episode011.rpy:546
translate german ep011_eva_talk_ca254246:

    # e "The warrior caste can provide one and if they’re unwilling I can requisition one."
    e "Die Kriegerkaste kann einen stellen, und wenn sie nicht wollen, kann ich einen anfordern."

# game/episode011.rpy:548
translate german ep011_eva_talk_f51e11e4:

    # av "Won’t that raise questions?"
    av "Wirft das nicht Fragen auf?"

# game/episode011.rpy:550
translate german ep011_eva_talk_4af92756:

    # e "Probably, but I’m the Queen."
    e "Wahrscheinlich schon, aber ich bin die Königin."

# game/episode011.rpy:552
translate german ep011_eva_talk_bfae1c43:

    # av "Point taken."
    av "Verstanden."

# game/episode011.rpy:554
translate german ep011_eva_talk_252ab192:

    # e "Let me know when everything is ready."
    e "Sag mir Bescheid, wenn alles fertig ist."

# game/episode011.rpy:555
translate german ep011_eva_talk_6dfceefe:

    # e "I’d really like to talk to Lilly in private now."
    e "Ich würde jetzt wirklich gerne mit Lilly unter vier Augen sprechen."

# game/episode011.rpy:556
translate german ep011_eva_talk_c313249e:

    # c "Of course, we’ll give you some alone time."
    c "Selbstverständlich werden wir dir etwas Zeit für dich allein geben."

# game/episode011.rpy:563
translate german ep011_vess_talk_1062e749:

    # "Vess was held in one of the cells the Acarhyn put us in when we first arrived."
    "Vess wurde in einer der Zellen festgehalten, in die die Acarhyn uns bei unserer Ankunft gesteckt hatten."

# game/episode011.rpy:565
translate german ep011_vess_talk_8fb43e3b:

    # "She looked miserable."
    "Sie sah elend aus."

# game/episode011.rpy:566
translate german ep011_vess_talk_f26dd47b:

    # "The Acarhyn hadn’t mistreated the girl, but she still bore the bruises from when she was found out in that communications tower."
    "Die Acarhyn hatten das Mädchen nicht misshandelt, aber sie trug immer noch die blauen Flecken davon, als sie in dem Kommunikationsturm entdeckt worden war."

# game/episode011.rpy:568
translate german ep011_vess_talk_1539c580:

    # "She suddenly seemed aware of my presence."
    "Plötzlich schien sie meine Anwesenheit zu bemerken."

# game/episode011.rpy:570
translate german ep011_vess_talk_aa1ba1c3:

    # ve "[p_name]! I-"
    ve "[p_name]! Ich-"

# game/episode011.rpy:574
translate german ep011_vess_talk_db096f3d:

    # c "Shut the fuck up."
    c "Halt die Fresse."

# game/episode011.rpy:575
translate german ep011_vess_talk_7dee55b7:

    # c "I want answers, so keep all that blubbering to yourself, traitor."
    c "Ich will Antworten, also behalt dein ganzes Gejammer für dich, Verräterin."

# game/episode011.rpy:576
translate german ep011_vess_talk_23b185dc:

    # "Vess lowered her head in shame again."
    "Vess senkte beschämt erneut den Kopf."

# game/episode011.rpy:578
translate german ep011_vess_talk_f00b84a8:

    # "I allowed her to finish her sentence, but it trailed off and she lowered her head again."
    "Ich ließ sie ihren Satz beenden, aber er verstummte und sie senkte wieder den Kopf."

# game/episode011.rpy:579
translate german ep011_vess_talk_e8bdbc17:

    # c "Why?!{w} Why did you betray us?"
    c "Warum?!{w} Warum hast du uns verraten?"

# game/episode011.rpy:581
translate german ep011_vess_talk_ad75cf2f:

    # ve "I didn’t want to.{w} At least, not anymore.{w} But I...{w} I..."
    ve "Ich wollte nicht.{w} Zumindest nicht mehr.{w} Aber ich...{w} Ich..."

# game/episode011.rpy:582
translate german ep011_vess_talk_52a1606b:

    # "She was on the verge of breaking down again."
    "Sie stand kurz davor, erneut zusammenzubrechen."

# game/episode011.rpy:586
translate german ep011_vess_talk_c6d0109d:

    # c "Stop stammering!{w} Tell me what happened!"
    c "Hör auf zu stottern!{w} Erzähl mir, was passiert ist!"

# game/episode011.rpy:588
translate german ep011_vess_talk_4f4951fd:

    # c "Take your time, but I really need to know what happened."
    c "Lass dir Zeit, aber ich muss unbedingt wissen, was passiert ist."

# game/episode011.rpy:589
translate german ep011_vess_talk_e32d8cbb:

    # "It took a while for her to compose herself, but when she looked up again at me I saw a modicum of resolve in her eyes."
    "Es dauerte eine Weile, bis sie sich wieder gefasst hatte, aber als sie wieder zu mir aufblickte, sah ich einen Anflug von Entschlossenheit in ihren Augen."

# game/episode011.rpy:591
translate german ep011_vess_talk_c4ca8c09:

    # ve "It wasn’t a coincidence I was on Lanan."
    ve "Es war kein Zufall, dass ich auf Lanan war."

# game/episode011.rpy:592
translate german ep011_vess_talk_7b036d22:

    # ve "I was never a villager, but just happened to be in the village when all hell broke loose."
    ve "Ich war nie ein Dorfbewohner, sondern befand mich zufällig im Dorf, als die Hölle losbrach."

# game/episode011.rpy:593
translate german ep011_vess_talk_f686d892:

    # ve "You finding me there was just a stroke of luck, so to say."
    ve "Dass du mich dort gefunden hast, war sozusagen reiner Zufall."

# game/episode011.rpy:594
translate german ep011_vess_talk_d95ca0c1:

    # c "Luck?"
    c "Glück?"

# game/episode011.rpy:595
translate german ep011_vess_talk_c085fd68:

    # ve "I was sent to observe you."
    ve "Ich wurde geschickt, um dich zu beobachten."

# game/episode011.rpy:598
translate german ep011_vess_talk_43965366:

    # ve "Or rather, your sister."
    ve "Oder besser gesagt, deine Schwester."

# game/episode011.rpy:599
translate german ep011_vess_talk_7df34f95:

    # c "Lilly?"
    c "Lilly?"

# game/episode011.rpy:600
translate german ep011_vess_talk_5d3e0033:

    # ve "No, Eva."
    ve "Nein, Eva."

# game/episode011.rpy:602
translate german ep011_vess_talk_da862084:

    # ve "Or rather, Eva."
    ve "Oder besser gesagt, Eva."

# game/episode011.rpy:603
translate german ep011_vess_talk_4e5a4202:

    # c "Why?"
    c "Warum?"

# game/episode011.rpy:605
translate german ep011_vess_talk_b5082579:

    # ve "Because she’s special."
    ve "Weil sie etwas Besonderes ist."

# game/episode011.rpy:606
translate german ep011_vess_talk_d6e24624:

    # c "Special how?"
    c "Was ist denn das Besondere daran?"

# game/episode011.rpy:607
translate german ep011_vess_talk_5beeb316:

    # ve "Eva is supposedly the subject of some prophecy."
    ve "Eva ist angeblich Gegenstand einer Prophezeiung."

# game/episode011.rpy:608
translate german ep011_vess_talk_a8318121:

    # c "“In the dying light of triple suns...”"
    c "„Im sterbenden Licht dreifacher Sonnen...“"

# game/episode011.rpy:610
translate german ep011_vess_talk_10b792b6:

    # ve "You know about the prophecy too?"
    ve "Kennst du die Prophezeiung auch?"

# game/episode011.rpy:611
translate german ep011_vess_talk_d20ce979:

    # c "We got security footage of one of those purple-clad women muttering it over and over."
    c "Wir haben Aufnahmen von einer Überwachungskamera, auf denen eine dieser lila gekleideten Frauen es immer wieder vor sich hin murmelt."

# game/episode011.rpy:612
translate german ep011_vess_talk_b443cc6c:

    # c "You killed her, didn’t you?"
    c "Du hast sie getötet, nicht wahr?"

# game/episode011.rpy:614
translate german ep011_vess_talk_51d69ee8:

    # ve "No...{w} But I gave her the knife that she used to commit suicide."
    ve "Nein...{w} Aber ich habe ihr das Messer gegeben, mit dem sie Selbstmord begangen hat."

# game/episode011.rpy:615
translate german ep011_vess_talk_638afbeb:

    # c "So you knew each other?"
    c "Ihr kanntet euch also?"

# game/episode011.rpy:617
translate german ep011_vess_talk_a67e40ab:

    # ve "Not exactly, but we follow the same master."
    ve "Nicht ganz, aber wir folgen demselben Meister."

# game/episode011.rpy:618
translate german ep011_vess_talk_199aa782:

    # c "Who?"
    c "Wer?"

# game/episode011.rpy:619
translate german ep011_vess_talk_4a5a961a:

    # ve "His name is the Confessor, or at least, that’s the only name I know him by."
    ve "Sein Name ist der Bekenner, oder zumindest ist das der einzige Name, unter dem ich ihn kenne."

# game/episode011.rpy:620
translate german ep011_vess_talk_33779e81:

    # ve "The women in purple are his handmaidens."
    ve "Die Frauen in Lila sind seine Dienerinnen."

# game/episode011.rpy:621
translate german ep011_vess_talk_24481d60:

    # c "And you Vess, who are you, apart from a spy and a traitor?"
    c "Und du, Vess, wer bist du, außer einer Spionin und Verräterin?"

# game/episode011.rpy:623
translate german ep011_vess_talk_6fc77004:

    # ve "I’m not a handmaiden, my mother was."
    ve "Ich bin keine Magd, meine Mutter war es."

# game/episode011.rpy:624
translate german ep011_vess_talk_f8278ede:

    # ve "After my parents were killed, the Confessor took me under his wing."
    ve "Nachdem meine Eltern getötet worden waren, nahm mich der Bekenner unter seine Fittiche."

# game/episode011.rpy:627
translate german ep011_vess_talk_9d7f75ea:

    # ve "He called me his granddaughter and that’s what I believed I was until I got old enough to know better."
    ve "Er nannte mich seine Enkelin, und ich glaubte auch, dass ich das war, bis ich alt genug war, es besser zu wissen."

# game/episode011.rpy:628
translate german ep011_vess_talk_975f19b9:

    # c "Why did he take such an interest in you?"
    c "Warum hat er sich so für dich interessiert?"

# game/episode011.rpy:630
translate german ep011_vess_talk_ee65e517:

    # ve "I’m not sure."
    ve "Ich bin mir nicht sicher."

# game/episode011.rpy:631
translate german ep011_vess_talk_a9e39c6a:

    # ve "Handmaidens are not allowed to have children, but my mother met my father on an expedition and she lapsed..."
    ve "Dienerinnen dürfen keine Kinder bekommen, aber meine Mutter lernte meinen Vater auf einer Expedition kennen und sie wurde rückfällig..."

# game/episode011.rpy:632
translate german ep011_vess_talk_3588900b:

    # ve "After I was born, both were executed on orders of the Confessor."
    ve "Nachdem ich geboren war, wurden beide auf Befehl des Bekenners hingerichtet."

# game/episode011.rpy:637
translate german ep011_vess_talk_bb94d892:

    # c "That’s brutal, I’m so sorry to hear that."
    c "Das ist ja furchtbar, das tut mir sehr leid."

# game/episode011.rpy:639
translate german ep011_vess_talk_4187cda3:

    # ve "Thank you."
    ve "Danke."

# game/episode011.rpy:640
translate german ep011_vess_talk_d7e8e8d1:

    # ve "It happened a long time ago and I didn’t learn about it until much later."
    ve "Das ist schon lange her und ich habe erst viel später davon erfahren."

# game/episode011.rpy:641
translate german ep011_vess_talk_19d06d6a:

    # ve "Still, it was a gut punch when the Confessor told me."
    ve "Trotzdem war es ein schwerer Schlag, als der Beichtvater es mir sagte."

# game/episode011.rpy:643
translate german ep011_vess_talk_fd955c20:

    # c "So you came to live with the Confessor at an early age?"
    c "Du bist also schon in jungen Jahren zum Bekenner gezogen?"

# game/episode011.rpy:645
translate german ep011_vess_talk_e79ef7be:

    # ve "I was groomed for my role as a spy from an early age."
    ve "Ich wurde von klein auf auf meine Rolle als Spion vorbereitet."

# game/episode011.rpy:646
translate german ep011_vess_talk_1f647cf8:

    # ve "Lanan was my first mission and following your movements would be my only task."
    ve "Lanan war meine erste Mission, und deine Bewegungen zu verfolgen, wäre meine einzige Aufgabe."

# game/episode011.rpy:647
translate german ep011_vess_talk_b36e8481:

    # ve "I was his sharpest knife, or so he told me."
    ve "Ich war sein schärfstes Messer, so sagte er mir."

# game/episode011.rpy:649
translate german ep011_vess_talk_202a8f2d:

    # ve "I’m not proud of what I’ve done."
    ve "Ich bin nicht stolz auf das, was ich getan habe."

# game/episode011.rpy:651
translate german ep011_vess_talk_7579a087:

    # c "You still haven’t explained why you betrayed us all."
    c "Du hast immer noch nicht erklärt, warum du uns alle verraten hast."

# game/episode011.rpy:653
translate german ep011_vess_talk_38e5bb34:

    # ve "I...{w} At first it all seemed like an adventure."
    ve "Ich...{w} Zuerst schien alles wie ein Abenteuer."

# game/episode011.rpy:654
translate german ep011_vess_talk_22d1dd48:

    # ve "Going to Lanan, observing the local population, watching those Sovereignty transports touch down all over the planet."
    ve "Nach Lanan gehen, die dortige Bevölkerung beobachten, beobachten, wie die Sovereignty-Transporter überall auf dem Planeten landen."

# game/episode011.rpy:655
translate german ep011_vess_talk_7dbd1fb7:

    # ve "When the Acarhyn came and butchered so many, it didn’t seem so adventurous anymore."
    ve "Als die Acarhyn kamen und so viele abschlachteten, schien es nicht mehr so ​​abenteuerlich."

# game/episode011.rpy:656
translate german ep011_vess_talk_10f1d4d7:

    # ve "But I had been given a mission and was required to carry it out."
    ve "Aber ich hatte einen Auftrag erhalten und war verpflichtet, ihn auszuführen."

# game/episode011.rpy:658
translate german ep011_vess_talk_252f3d9d:

    # ve "When I finally made contact with you, everything changed."
    ve "Als ich endlich Kontakt zu dir aufgenommen habe, hat sich alles verändert."

# game/episode011.rpy:661
translate german ep011_vess_talk_c14c3d8e:

    # ve "You changed me."
    ve "Du hast mich verändert."

# game/episode011.rpy:664
translate german ep011_vess_talk_ab482aa5:

    # ve "The way you cast everything aside to find your sister, it really struck me."
    ve "Wie du alles beiseite geschoben hast, um deine Schwester zu finden, hat mich wirklich beeindruckt."

# game/episode011.rpy:666
translate german ep011_vess_talk_de30f6da:

    # ve "The way you cast everything aside to find Eva, it really struck me."
    ve "Wie du alles andere beiseite geschoben hast, um Eva zu finden, hat mich wirklich beeindruckt."

# game/episode011.rpy:667
translate german ep011_vess_talk_9f41d1df:

    # ve "Then, when I became part of the crew, I experienced the warmth and companionship I’d never felt before."
    ve "Als ich dann Teil der Crew wurde, erlebte ich eine Wärme und Kameradschaft, die ich vorher noch nie gespürt hatte."

# game/episode011.rpy:668
translate german ep011_vess_talk_d4609160:

    # ve "At first, I dutifully reported my findings back to the Confessor."
    ve "Zunächst habe ich dem Beichtvater pflichtbewusst meine Erkenntnisse mitgeteilt."

# game/episode011.rpy:669
translate german ep011_vess_talk_637f8b1d:

    # c "Yes, Céline found out about those."
    c "Ja, Céline hat davon erfahren."

# game/episode011.rpy:670
translate german ep011_vess_talk_a17ab609:

    # ve "I knew it would only be a matter of time before she would."
    ve "Ich wusste, dass es nur eine Frage der Zeit sein würde, bis sie es tun würde."

# game/episode011.rpy:671
translate german ep011_vess_talk_6619f720:

    # ve "At a certain point I couldn’t do it anymore."
    ve "Irgendwann ging es für mich einfach nicht mehr."

# game/episode011.rpy:673
translate german ep011_vess_talk_0576ce92:

    # ve "You all treated me with such kindness, accepted me without question..."
    ve "Ihr alle wart so freundlich zu mir, habt mich ohne Fragen akzeptiert..."

# game/episode011.rpy:674
translate german ep011_vess_talk_381aaaca:

    # c "And yet you still betrayed us."
    c "Und trotzdem hast du uns verraten."

# game/episode011.rpy:676
translate german ep011_vess_talk_e2dd8350:

    # ve "Yes...{w} The Confessor he told me...{w} My mother...{w} She’s still alive."
    ve "Ja...{w} Der Beichtvater sagte mir...{w} Meine Mutter...{w} Sie lebt noch."

# game/episode011.rpy:677
translate german ep011_vess_talk_5216dbb9:

    # c "And you believe that?"
    c "Und das glaubst du?"

# game/episode011.rpy:678
translate german ep011_vess_talk_a81200f5:

    # ve "I have to! He showed me a picture of her!"
    ve "Ich muss! Er hat mir ein Foto von ihr gezeigt!"

# game/episode011.rpy:679
translate german ep011_vess_talk_e4f1b063:

    # c "So that’s why you turned us in?"
    c "Also deshalb hast du uns verraten?"

# game/episode011.rpy:681
translate german ep011_vess_talk_c18eaada:

    # ve "Yes...{w} I..."
    ve "Ja...{w} Ich..."

# game/episode011.rpy:686
translate german ep011_vess_talk_26e0b4f4:

    # c "Why didn’t you come to us, come to me?"
    c "Warum bist du nicht zu uns gekommen, zu mir?"

# game/episode011.rpy:687
translate german ep011_vess_talk_68afca74:

    # c "You know how resourceful we are, we could have come up with a plan."
    c "Du weißt doch, wie einfallsreich wir sind, wir hätten uns einen Plan ausdenken können."

# game/episode011.rpy:688
translate german ep011_vess_talk_4c6383de:

    # c "Instead you went behind our backs."
    c "Stattdessen hast du hinter unserem Rücken gehandelt."

# game/episode011.rpy:690
translate german ep011_vess_talk_8a895116:

    # c "How could you be so selfish?!"
    c "Wie konntest du nur so egoistisch sein?!"

# game/episode011.rpy:693
translate german ep011_vess_talk_52f5c276:

    # c "You stupid self-centered cunt!"
    c "Du dumme, egozentrische Schlampe!"

# game/episode011.rpy:694
translate german ep011_vess_talk_a4eea8b5:

    # c "Do you know in how much danger you’ve put us?!"
    c "Weißt du, in welch große Gefahr du uns gebracht hast?!"

# game/episode011.rpy:695
translate german ep011_vess_talk_086af638:

    # c "And all for some sentimental pipe dream about the mother you’ve never known!"
    c "Und das alles wegen einer sentimentalen Hirngespinst über die Mutter, die du nie kennengelernt hast!"

# game/episode011.rpy:697
translate german ep011_vess_talk_5338454d:

    # ve "I know, I know..."
    ve "Ich weiß, ich weiß..."

# game/episode011.rpy:699
translate german ep011_vess_talk_940b3f05:

    # ve "At the time it didn’t feel like I had a choice."
    ve "Damals hatte ich das Gefühl, keine Wahl zu haben."

# game/episode011.rpy:700
translate german ep011_vess_talk_bba8a9af:

    # ve "You don’t know the Confessor like I do, [p_name], the power he wields!"
    ve "Du kennst den Beichtvater nicht so gut wie ich, [p_name], welche Macht er besitzt!"

# game/episode011.rpy:701
translate german ep011_vess_talk_232c0e3e:

    # c "What, he’s some sort of wizard?"
    c "Was, ist er etwa ein Zauberer?"

# game/episode011.rpy:702
translate german ep011_vess_talk_aae45f05:

    # c "Can he shoot lightning from his fingertips and kill you with his mind?"
    c "Kann er Blitze aus seinen Fingerspitzen schießen und dich mit seinen Gedanken töten?"

# game/episode011.rpy:704
translate german ep011_vess_talk_d5def737:

    # ve "No."
    ve "Nein."

# game/episode011.rpy:705
translate german ep011_vess_talk_4eda3157:

    # c "So he’s just a man."
    c "Also ist er nur ein Mensch."

# game/episode011.rpy:707
translate german ep011_vess_talk_013649e9:

    # ve "A man who still wields a lot of power."
    ve "Ein Mann, der immer noch viel Macht besitzt."

# game/episode011.rpy:708
translate german ep011_vess_talk_e5882bc4:

    # c "No doubt."
    c "Ohne Zweifel."

# game/episode011.rpy:710
translate german ep011_vess_talk_b8ffcbb2:

    # c "You should have come to me though."
    c "Du hättest aber zu mir kommen sollen."

# game/episode011.rpy:712
translate german ep011_vess_talk_6f68fb84:

    # ve "I’m sorry, [p_name]."
    ve "Es tut mir leid, [p_name]."

# game/episode011.rpy:713
translate german ep011_vess_talk_1b5081cc:

    # c "When you’ve stacked lie upon lie, it’s very hard to come clean."
    c "Wenn man Lüge auf Lüge gehäuft hat, ist es sehr schwer, reinen Tisch zu machen."

# game/episode011.rpy:715
translate german ep011_vess_talk_05bd74de:

    # ve "I knew you’d be hurt."
    ve "Ich wusste, dass du verletzt sein würdest."

# game/episode011.rpy:716
translate german ep011_vess_talk_e96e669c:

    # ve "Like you’re hurt now."
    ve "Als wärst du jetzt verletzt."

# game/episode011.rpy:717
translate german ep011_vess_talk_c513d95d:

    # ve "I’m so stupid..."
    ve "Ich bin so dumm..."

# game/episode011.rpy:718
translate german ep011_vess_talk_5b41daa0:

    # ve "I’ve had feelings for you from the moment I really saw you in the medbay of the Ypotryll."
    ve "Ich habe Gefühle für dich, seit ich dich das erste Mal in der Krankenstation der Ypotryll gesehen habe."

# game/episode011.rpy:719
translate german ep011_vess_talk_065f2ef8:

    # ve "I tried to contain it, but you made that especially hard."
    ve "Ich habe versucht, es zu unterdrücken, aber du hast es mir besonders schwer gemacht."

# game/episode011.rpy:721
translate german ep011_vess_talk_d4d7d978:

    # ve "I know that what I’ve done is unforgivable and that I’ve made the wrong choice at every turn."
    ve "Ich weiß, dass das, was ich getan habe, unverzeihlich ist und dass ich bei jeder Gelegenheit die falsche Entscheidung getroffen habe."

# game/episode011.rpy:722
translate german ep011_vess_talk_e1a11b91:

    # ve "I don’t know how I can make amends, but know that I want to."
    ve "Ich weiß nicht, wie ich das wiedergutmachen kann, aber ich möchte es unbedingt."

# game/episode011.rpy:723
translate german ep011_vess_talk_48047743:

    # c "No matter the cost?"
    c "Koste es, was es wolle?"

# game/episode011.rpy:725
translate german ep011_vess_talk_43759077:

    # ve "No matter the cost."
    ve "Egal was es kostet."

# game/episode011.rpy:726
translate german ep011_vess_talk_982b3994:

    # c "Erylin has gone to meet this Confessor of yours."
    c "Erylin ist zu deinem Beichtvater gegangen."

# game/episode011.rpy:727
translate german ep011_vess_talk_450235ab:

    # c "His ship has landed at the other side of the planet."
    c "Sein Schiff ist auf der anderen Seite des Planeten gelandet."

# game/episode011.rpy:728
translate german ep011_vess_talk_85daf14c:

    # c "Remarkably, the ground-to-air defences didn’t pick up the massive ship when it entered orbit."
    c "Erstaunlicherweise wurde das riesige Schiff von der Boden-Luft-Abwehr nicht erfasst, als es in die Umlaufbahn eintrat."

# game/episode011.rpy:729
translate german ep011_vess_talk_2c23497b:

    # c "Your handiwork, I presume?"
    c "Das ist wohl dein Werk?"

# game/episode011.rpy:731
translate german ep011_vess_talk_1212d107:

    # ve "Yes..."
    ve "Ja..."

# game/episode011.rpy:732
translate german ep011_vess_talk_8a0b4a58:

    # c "I’m guessing this Confessor can’t be reasoned with?"
    c "Ich nehme an, mit diesem Beichtvater kann man nicht vernünftig reden?"

# game/episode011.rpy:734
translate german ep011_vess_talk_cdf2d08c:

    # ve "No, he’s only after one thing.{w} Eva."
    ve "Nein, er ist nur hinter einer Sache her.{w} Eva."

# game/episode011.rpy:735
translate german ep011_vess_talk_9dcac795:

    # ve "He’ll stop at nothing to get what he wants."
    ve "Er schreckt vor nichts zurück, um zu bekommen, was er will."

# game/episode011.rpy:736
translate german ep011_vess_talk_cb5d9961:

    # c "What will he do with her?"
    c "Was wird er mit ihr machen?"

# game/episode011.rpy:738
translate german ep011_vess_talk_bd084452:

    # ve "I’m not sure, experiments?"
    ve "Ich bin mir nicht sicher, Experimente?"

# game/episode011.rpy:739
translate german ep011_vess_talk_c12ec7dc:

    # ve "All I know is she’s supposed to be a key to some great prophesied event."
    ve "Ich weiß nur, dass sie angeblich der Schlüssel zu einem großen, prophezeiten Ereignis sein soll."

# game/episode011.rpy:740
translate german ep011_vess_talk_ef473030:

    # c "You’re going to have to explain me the details later."
    c "Du musst mir die Details später erklären."

# game/episode011.rpy:742
translate german ep011_vess_talk_babc17bb:

    # ve "Of course, I’ll tell you everything I know."
    ve "Natürlich werde ich dir alles erzählen, was ich weiß."

# game/episode011.rpy:743
translate german ep011_vess_talk_fe04b124:

    # c "So, the Confessor is coming and you’re saying Erylin doesn’t stand a chance against him?"
    c "Also kommt der Bekenner und du meinst, Erylin hat keine Chance gegen ihn?"

# game/episode011.rpy:744
translate german ep011_vess_talk_adc9370a:

    # ve "He’ll either lure her with something she wants or crush her."
    ve "Er wird sie entweder mit etwas locken, das sie begehrt, oder sie vernichten."

# game/episode011.rpy:745
translate german ep011_vess_talk_e8b88fdb:

    # c "Based on my short acquaintance with Erylin, she’ll go with the first option."
    c "Nach meiner kurzen Bekanntschaft mit Erylin wird sie sich für die erste Option entscheiden."

# game/episode011.rpy:747
translate german ep011_vess_talk_eb257057:

    # c "So we have some time before she returns to get Eva to come with us."
    c "Wir haben also noch etwas Zeit, bevor sie zurückkommt, um Eva dazu zu bringen, mit uns zu kommen."

# game/episode011.rpy:748
translate german ep011_vess_talk_ea9d72f5:

    # c "I’ll make sure you get transferred to the Bastard’s brig."
    c "Ich sorge dafür, dass du in den Arrest des Bastards verlegt wirst."

# game/episode011.rpy:750
translate german ep011_vess_talk_b62c291a:

    # ve "Thank you, [p_name]."
    ve "Danke, [p_name]."

# game/episode011.rpy:751
translate german ep011_vess_talk_bf4c5150:

    # c "Don’t thank me yet, I still haven’t decided what to do with you afterwards."
    c "Bedank dich noch nicht, ich habe noch nicht entschieden, was ich danach mit dir mache."

# game/episode011.rpy:752
translate german ep011_vess_talk_53eed0e9:

    # ve "Fair enough."
    ve "Das stimmt schon."

# game/episode011.rpy:755
translate german ep011_vess_talk_8fb9ce4c:

    # ve "[p_name], what’s going to happen to me?"
    ve "[p_name], was wird mit mir geschehen?"

# game/episode011.rpy:756
translate german ep011_vess_talk_299d153d:

    # c "I don’t know."
    c "Ich weiß es nicht."

# game/episode011.rpy:757
translate german ep011_vess_talk_2ac7bef9:

    # c "You’re at the mercy of the Acarhyn now."
    c "Du bist jetzt den Acarhyn ausgeliefert."

# game/episode011.rpy:758
translate german ep011_vess_talk_1986694c:

    # c "I’d continue to be helpful, if I were you."
    c "Ich würde an deiner Stelle weiterhin hilfsbereit sein."

# game/episode011.rpy:759
translate german ep011_vess_talk_0e168e7d:

    # ve "I...{w} I understand."
    ve "Ich...{w} Ich verstehe."

# game/episode011.rpy:761
translate german ep011_vess_talk_d63a62cd:

    # "I left the girl I thought I knew in the Acarhyn dungeon."
    "Ich habe das Mädchen, das ich zu kennen glaubte, im Kerker von Acarhyn zurückgelassen."

# game/episode011.rpy:762
translate german ep011_vess_talk_d47c37c6:

    # "She didn’t cry out when I left, but her desperation was almost palpable."
    "Sie schrie nicht auf, als ich ging, aber ihre Verzweiflung war fast greifbar."

# game/episode011.rpy:769
translate german ep011_rahia_talk_82ee31da:

    # "Considering what Vess had told me, I thought it best to consult Rahia."
    "Angesichts dessen, was Vess mir erzählt hatte, hielt ich es für das Beste, Rahia zu konsultieren."

# game/episode011.rpy:771
translate german ep011_rahia_talk_a94f4152:

    # "She owed me no loyalty, but maybe the well-being of her Queen could persuade her."
    "Sie war mir zu keinerlei Loyalität verpflichtet, aber vielleicht könnte das Wohlergehen ihrer Königin sie umstimmen."

# game/episode011.rpy:773
translate german ep011_rahia_talk_05917663:

    # rah "Well, if it isn’t the Hero of the Acarhyn."
    rah "Na sowas, wenn das nicht der Held von Acarhyn ist."

# game/episode011.rpy:774
translate german ep011_rahia_talk_bdb5aa0f:

    # c "In the flesh."
    c "Leibhaftig."

# game/episode011.rpy:777
translate german ep011_rahia_talk_e7f557eb:

    # rah "Thank you for siding with us in this whole event."
    rah "Danke, dass du uns in dieser ganzen Angelegenheit unterstützt hast."

# game/episode011.rpy:778
translate german ep011_rahia_talk_4e8c7062:

    # c "Of course."
    c "Natürlich."

# game/episode011.rpy:779
translate german ep011_rahia_talk_4061ac74:

    # rah "Seems like this new situation with the Confessor upsets the whole balance again."
    rah "Es scheint, als ob diese neue Situation mit dem Beichtvater das gesamte Gleichgewicht erneut durcheinanderbringt."

# game/episode011.rpy:780
translate german ep011_rahia_talk_8a7e31e7:

    # rah "Erylin was so eager to visit that landing site, I had to scramble to get a contingent of warriors to go with her."
    rah "Erylin wollte diesen Landeplatz unbedingt besuchen, deshalb musste ich mich beeilen, eine Gruppe Krieger zusammenzutrommeln, die sie begleiten sollten."

# game/episode011.rpy:781
translate german ep011_rahia_talk_3cd42e61:

    # c "She’s smelled an opportunity, that much is clear."
    c "Sie hat ihre Chance gewittert, so viel ist klar."

# game/episode011.rpy:782
translate german ep011_rahia_talk_8f9c7912:

    # rah "Surely she wouldn’t go as far as selling out the Queen."
    rah "Sie würde doch nicht so weit gehen, die Königin zu verraten."

# game/episode011.rpy:785
translate german ep011_rahia_talk_13153c1a:

    # rah "What do you want, oathbreaker?"
    rah "Was willst du, Eidbrecher?"

# game/episode011.rpy:786
translate german ep011_rahia_talk_5029606a:

    # c "Look, Rahia, I-"
    c "Schau, Rahia, ich-"

# game/episode011.rpy:788
translate german ep011_rahia_talk_fb830f86:

    # rah "What do you want that your new friend Erylin can't handle?"
    rah "Was willst du, womit deine neue Freundin Erylin nicht umgehen kann?"

# game/episode011.rpy:789
translate german ep011_rahia_talk_7a3c0f40:

    # c "I just want to talk."
    c "Ich möchte einfach nur reden."

# game/episode011.rpy:791
translate german ep011_rahia_talk_81741946:

    # rah "Save me your speeches."
    rah "Erspar mir deine Reden."

# game/episode011.rpy:792
translate german ep011_rahia_talk_6a63fb68:

    # rah "I didn’t take you for a politician, that’s all."
    rah "Ich hätte dich nicht für eine Politikerin gehalten, das ist alles."

# game/episode011.rpy:793
translate german ep011_rahia_talk_c8226b3d:

    # rah "It’s as much a flaw in my judgment as it is one in your character."
    rah "Das ist ebenso sehr ein Fehler in meinem Urteilsvermögen wie in deinem Charakter."

# game/episode011.rpy:794
translate german ep011_rahia_talk_f5f7c27d:

    # c "Right."
    c "Richtig."

# game/episode011.rpy:796
translate german ep011_rahia_talk_1e86d568:

    # rah "You’ve only been granted the permission to see me because of your new title."
    rah "Du hast die Erlaubnis, mich zu sehen, nur aufgrund deines neuen Titels erhalten."

# game/episode011.rpy:797
translate german ep011_rahia_talk_3a040b62:

    # rah "I’ll hear you out and then I expect you to leave."
    rah "Ich höre dir zu und erwarte dann, dass du gehst."

# game/episode011.rpy:798
translate german ep011_rahia_talk_89d49e3f:

    # rah "So, what do you want?"
    rah "Also, was möchtest du?"

# game/episode011.rpy:799
translate german ep011_rahia_talk_71e550da:

    # c "The situation with the Confessor."
    c "Die Situation mit dem Beichtvater."

# game/episode011.rpy:800
translate german ep011_rahia_talk_c3ca0e51:

    # c "That’s what has me so worried."
    c "Genau das bereitet mir solche Sorgen."

# game/episode011.rpy:802
translate german ep011_rahia_talk_326a85a5:

    # rah "Is that so?"
    rah "Ist das so?"

# game/episode011.rpy:803
translate german ep011_rahia_talk_8c0ad80a:

    # c "I’m pretty certain Erylin is going to use this situation to her own advantage."
    c "Ich bin mir ziemlich sicher, dass Erylin diese Situation zu ihrem Vorteil nutzen wird."

# game/episode011.rpy:805
translate german ep011_rahia_talk_baeba38a:

    # rah "Besides, aren't you in league with her?"
    rah "Außerdem, bist du nicht mit ihr unter einer Decke?"

# game/episode011.rpy:806
translate german ep011_rahia_talk_b489c235:

    # c "She isn’t taking my calls, so to speak."
    c "Sie nimmt sozusagen meine Anrufe nicht entgegen."

# game/episode011.rpy:808
translate german ep011_rahia_talk_cc29f32e:

    # rah "Of course she isn’t."
    rah "Natürlich ist sie das nicht."

# game/episode011.rpy:809
translate german ep011_rahia_talk_b2dd190a:

    # c "It was very much a marriage of convenience."
    c "Es war in erster Linie eine Vernunftehe."

# game/episode011.rpy:811
translate german ep011_rahia_talk_df0666be:

    # rah "Above all else, Erylin is a political animal."
    rah "Erylin ist vor allem eines: ein politisches Tier."

# game/episode011.rpy:812
translate german ep011_rahia_talk_dd8c359e:

    # rah "Still, I doubt she’d sell out the Queen."
    rah "Trotzdem bezweifle ich, dass sie die Königin verraten würde."

# game/episode011.rpy:814
translate german ep011_rahia_talk_a314f509:

    # rah "By doing what, selling out the Queen?"
    rah "Indem du was getan hast, indem du die Königin verraten hast?"

# game/episode011.rpy:816
translate german ep011_rahia_talk_42827949:

    # rah "What brings you here?"
    rah "Was führt dich hierher?"

# game/episode011.rpy:817
translate german ep011_rahia_talk_16f833f5:

    # rah "I really don’t have the time for this."
    rah "Ich habe dafür wirklich keine Zeit."

# game/episode011.rpy:818
translate german ep011_rahia_talk_71e550da_1:

    # c "The situation with the Confessor."
    c "Die Situation mit dem Beichtvater."

# game/episode011.rpy:819
translate german ep011_rahia_talk_8ba21add:

    # rah "Right."
    rah "Richtig."

# game/episode011.rpy:820
translate german ep011_rahia_talk_66378561:

    # rah "Erylin and a contingent of warriors are dealing with that."
    rah "Erylin und eine Gruppe Krieger kümmern sich darum."

# game/episode011.rpy:821
translate german ep011_rahia_talk_c3ca0e51_1:

    # c "That’s what has me so worried."
    c "Genau das bereitet mir solche Sorgen."

# game/episode011.rpy:823
translate german ep011_rahia_talk_326a85a5_1:

    # rah "Is that so?"
    rah "Ist das so?"

# game/episode011.rpy:824
translate german ep011_rahia_talk_8c0ad80a_1:

    # c "I’m pretty certain Erylin is going to use this situation to her own advantage."
    c "Ich bin mir ziemlich sicher, dass Erylin diese Situation zu ihrem Vorteil nutzen wird."

# game/episode011.rpy:825
translate german ep011_rahia_talk_a314f509_1:

    # rah "By doing what, selling out the Queen?"
    rah "Indem du was getan hast, indem du die Königin verraten hast?"

# game/episode011.rpy:826
translate german ep011_rahia_talk_39f5dd6d:

    # c "I wouldn’t put it past her."
    c "Ich würde es ihr durchaus zutrauen."

# game/episode011.rpy:828
translate german ep011_rahia_talk_9c4fa9e2:

    # rah "I’m sorry, but for all her faults, Erylin is still very much a patriot."
    rah "Es tut mir leid, aber trotz all ihrer Fehler ist Erylin immer noch eine echte Patriotin."

# game/episode011.rpy:829
translate german ep011_rahia_talk_55fdd609:

    # c "That might be true, but power also corrupts."
    c "Das mag stimmen, aber Macht korrumpiert auch."

# game/episode011.rpy:830
translate german ep011_rahia_talk_a99818a5:

    # c "I think it would be safer for Eva not to be here when Erylin returns."
    c "Ich glaube, es wäre sicherer, wenn Eva nicht hier wäre, wenn Erylin zurückkehrt."

# game/episode011.rpy:832
translate german ep011_rahia_talk_214af63b:

    # rah "What are you suggesting?"
    rah "Was schlägst du vor?"

# game/episode011.rpy:833
translate german ep011_rahia_talk_3eb204ed:

    # c "I’m suggesting that the Queen leaves the Citadel and relocates to a safer place in case disaster strikes."
    c "Ich schlage vor, dass die Königin die Zitadelle verlässt und sich an einen sichereren Ort begibt, falls eine Katastrophe eintritt."

# game/episode011.rpy:834
translate german ep011_rahia_talk_3ec42731:

    # rah "There’s no reason to suppose-"
    rah "Es gibt keinen Grund, das anzunehmen."

# game/episode011.rpy:838
translate german ep011_rahia_talk_8d427fc5:

    # "One of the Acarhyn warriors burst into the room."
    "Einer der Acarhyn-Krieger stürmte in den Raum."

# game/episode011.rpy:841
translate german ep011_rahia_talk_9d1534a9:

    # woman "Forgive me the intrusion, but there's fighting in the Citadel."
    woman "Entschuldige die Störung, aber in der Zitadelle wird gekämpft."

# game/episode011.rpy:843
translate german ep011_rahia_talk_1ce22613:

    # rah "What?!"
    rah "Was?!"

# game/episode011.rpy:845
translate german ep011_rahia_talk_28a4c3f0:

    # woman "The priest caste suddenly turned on their fellow Acarhyn and are in the process of securing every major chokepoint."
    woman "Die Priesterkaste hat sich plötzlich gegen ihre eigenen Acarhyn-Bürger gewandt und ist dabei, alle wichtigen strategischen Punkte zu sichern."

# game/episode011.rpy:846
translate german ep011_rahia_talk_b4c39df5:

    # c "I think you've just been given your reason..."
    c "Ich glaube, du hast gerade deinen Grund erhalten..."

# game/episode011.rpy:848
translate german ep011_rahia_talk_ff40ec91:

    # rah "That traitorous-"
    rah "Dieser Verräter-"

# game/episode011.rpy:849
translate german ep011_rahia_talk_73fb5e0a:

    # rah "The Queen, is she...?"
    rah "Die Königin, ist sie es...?"

# game/episode011.rpy:851
translate german ep011_rahia_talk_e35281fb:

    # woman "Several of our warriors are with her."
    woman "Einige unserer Krieger sind bei ihr."

# game/episode011.rpy:853
translate german ep011_rahia_talk_911f0710:

    # rah "Has anybody tried to contact the warriors who're with Erylin?"
    rah "Hat schon jemand versucht, die Kriegerinnen, die mit Erylin zusammen sind, zu kontaktieren?"

# game/episode011.rpy:854
translate german ep011_rahia_talk_3900bc8d:

    # woman "Yes, but there's no response."
    woman "Ja, aber es kommt keine Antwort."

# game/episode011.rpy:855
translate german ep011_rahia_talk_62b2677f:

    # woman "According to our sources, Erylin is headed back to the Citadel."
    woman "Unseren Quellen zufolge kehrt Erylin zur Zitadelle zurück."

# game/episode011.rpy:856
translate german ep011_rahia_talk_59510e2b:

    # rah "We must get the Queen to safety."
    rah "Wir müssen die Königin in Sicherheit bringen."

# game/episode011.rpy:858
translate german ep011_rahia_talk_ab46f0c6:

    # "Somebody fired through the open door, hitting the Acarhyn warrior square in the back."
    "Jemand feuerte durch die offene Tür und traf den Acarhyn-Krieger mitten in den Rücken."

# game/episode011.rpy:860
translate german ep011_rahia_talk_e3b0238b:

    # "The woman collapsed, the hole in her spine still smoking."
    "Die Frau brach zusammen, aus dem Loch in ihrer Wirbelsäule qualmte noch immer."

# game/episode011.rpy:861
translate german ep011_rahia_talk_5321a75e:

    # "Rahia lunged for her sword and waited for the attackers to come spilling into her quarters."
    "Rahia griff nach ihrem Schwert und wartete darauf, dass die Angreifer in ihre Gemächer strömten."

# game/episode011.rpy:863
translate german ep011_rahia_talk_dedabfed:

    # "Three assailants eventually entered the commander's room, weapons at the ready."
    "Drei Angreifer drangen schließlich mit gezückten Waffen in das Zimmer des Kommandanten ein."

# game/episode011.rpy:864
translate german ep011_rahia_talk_7576c911:

    # "Though they were careful, they clearly hadn't counted on Rahia wielding her enormous sword."
    "Obwohl sie vorsichtig waren, hatten sie ganz offensichtlich nicht damit gerechnet, dass Rahia ihr riesiges Schwert schwingen würde."

# game/episode011.rpy:866
translate german ep011_rahia_talk_395ec050:

    # "Within seconds, one of the priestesses had collapsed to the floor, a large stab wound in her abdomen."
    "Innerhalb weniger Sekunden war eine der Priesterinnen zusammengebrochen; sie wies eine große Stichwunde im Bauch auf."

# game/episode011.rpy:867
translate german ep011_rahia_talk_44ca9f52:

    # "With a roar, Rahia whirled and sheared off the head of one of the other women."
    "Mit einem lauten Gebrüll wirbelte Rahia herum und schlug einer der anderen Frauen den Kopf ab."

# game/episode011.rpy:869
translate german ep011_rahia_talk_3e82450f:

    # "That left one attacker, who was slowly backing off towards the room's exit."
    "Damit blieb nur noch ein Angreifer übrig, der sich langsam in Richtung Ausgang des Raumes zurückzog."

# game/episode011.rpy:870
translate german ep011_rahia_talk_f29f3b46:

    # "Rahia's sword kept her engaged and soon the women were circling each other, the priestess wielding her glaive defensively."
    "Rahias Schwert hielt sie in Bewegung und bald umkreisten sich die Frauen, wobei die Priesterin ihre Gleve defensiv schwang."

# game/episode011.rpy:872
translate german ep011_rahia_talk_778e7970:

    # "Shots were again fired from the hallway, intended to end the fight quickly in the priestesses favor."
    "Aus dem Flur wurden erneut Schüsse abgefeuert, um den Kampf schnell zugunsten der Priesterinnen zu beenden."

# game/episode011.rpy:874
translate german ep011_rahia_talk_94b02507:

    # "I noticed a dagger lying on the table with Rahia's armor."
    "Mir fiel auf, dass auf dem Tisch neben Rahias Rüstung ein Dolch lag."

# game/episode011.rpy:875
translate german ep011_rahia_talk_6e55b406:

    # "The priestess was toying with Rahia, clearly drawing the fight out, so that the ranged attacks would finish the commander off."
    "Die Priesterin spielte mit Rahia, sie zog den Kampf offensichtlich in die Länge, damit die Fernangriffe den Kommandanten schließlich erledigen würden."

# game/episode011.rpy:877
translate german ep011_rahia_talk_2badf7da:

    # "Grabbing the dagger, I made my careful approach towards the warrior and the priestess."
    "Ich griff nach dem Dolch und näherte mich vorsichtig dem Krieger und der Priesterin."

# game/episode011.rpy:878
translate german ep011_rahia_talk_0769b6b8:

    # "Another salvo of energy bolts nearly struck me, but I managed to dart out of the way."
    "Ein weiterer Salve von Energieblitzen verfehlte mich nur knapp, aber ich konnte mich gerade noch rechtzeitig ausweichen."

# game/episode011.rpy:879
translate german ep011_rahia_talk_67600b72:

    # "That's when I saw the opening."
    "Da sah ich die Eröffnung."

# game/episode011.rpy:881
translate german ep011_rahia_talk_d02e641e:

    # "As the priestess raised her weapon, I rammed the dagger into her exposed flesh."
    "Als die Priesterin ihre Waffe hob, rammte ich ihr den Dolch in ihr entblößtes Fleisch."

# game/episode011.rpy:882
translate german ep011_rahia_talk_d3cb3123:

    # "She screamed and nearly dropped the glaive from her grasp."
    "Sie schrie auf und ließ beinahe die Gleve aus ihrer Hand fallen."

# game/episode011.rpy:884
translate german ep011_rahia_talk_eb8366b8:

    # "The distraction was all Rahia needed to stab the priestess in the gut."
    "Diese Ablenkung war alles, was Rahia brauchte, um der Priesterin in den Bauch zu stechen."

# game/episode011.rpy:885
translate german ep011_rahia_talk_0e83a796:

    # "The woman collapsed gurgling to the floor."
    "Die Frau brach gurgelnd zu Boden zusammen."

# game/episode011.rpy:886
translate german ep011_rahia_talk_c9636083:

    # "I had time to pick up both staffs of the women who were killed earlier and tossed one to Rahia."
    "Ich hatte noch Zeit, die beiden Stäbe der zuvor getöteten Frauen aufzuheben und einen davon Rahia zuzuwerfen."

# game/episode011.rpy:888
translate german ep011_rahia_talk_a1e95aaa:

    # "The commander caught the staff and quickly took up position at the side of the door."
    "Der Kommandant ergriff den Stab und bezog schnell Stellung neben der Tür."

# game/episode011.rpy:889
translate german ep011_rahia_talk_5e526fb7:

    # "I moved towards the other side and dared to look inside the hallway."
    "Ich ging auf die andere Seite und wagte es, in den Flur hineinzuschauen."

# game/episode011.rpy:891
translate german ep011_rahia_talk_4a8a75e8:

    # "Two more Acarhyn had crawled behind pieces of fallen furniture, they fired as soon as they saw my face."
    "Zwei weitere Acarhyn hatten sich hinter umgestürzten Möbelstücken versteckt; sie eröffneten das Feuer, sobald sie mein Gesicht sahen."

# game/episode011.rpy:892
translate german ep011_rahia_talk_93944386:

    # "I was barely able to jerk my head back when the volley of bolts whizzed past us and violently struck the wall."
    "Ich konnte meinen Kopf kaum zurückreißen, als die Salve von Bolzen an uns vorbeizischte und mit voller Wucht gegen die Wand prallte."

# game/episode011.rpy:894
translate german ep011_rahia_talk_7366e70a:

    # c "Two attackers, one on the left, one on the right."
    c "Zwei Angreifer, einer links, einer rechts."

# game/episode011.rpy:895
translate german ep011_rahia_talk_4fa290f8:

    # rah "Right, we'll take them both at once."
    rah "Okay, wir nehmen beide auf einmal."

# game/episode011.rpy:896
translate german ep011_rahia_talk_5535ba77:

    # rah "You fire on the left one, I'll take care of the other cunt."
    rah "Du feuerst auf die linke, ich kümmere mich um die andere Schlampe."

# game/episode011.rpy:897
translate german ep011_rahia_talk_7963cf59:

    # c "Understood."
    c "Verstanden."

# game/episode011.rpy:898
translate german ep011_rahia_talk_baa86027:

    # rah "On three."
    rah "Auf drei."

# game/episode011.rpy:899
translate german ep011_rahia_talk_3eff8dab:

    # rah "One.{w} Two.{w} Three."
    rah "Eins.{w} Zwei.{w} Drei."

# game/episode011.rpy:901
translate german ep011_rahia_talk_71142321:

    # "We stepped into the door opening, and before our assailants could raise their staffs, we fired multiple bolts towards them."
    "Wir traten in die Türöffnung, und bevor unsere Angreifer ihre Stöcke heben konnten, feuerten wir mehrere Bolzen auf sie ab."

# game/episode011.rpy:902
translate german ep011_rahia_talk_1e0c2aae:

    # "One of my bolts took the priestess full in the face."
    "Einer meiner Bolzen traf die Priesterin voll ins Gesicht."

# game/episode011.rpy:904
translate german ep011_rahia_talk_ac32ad6e:

    # "Rahia dashed towards the barricade, swinging her sword and attacked the remaining priestess, who fell to the floor in a spray of blood."
    "Rahia stürmte auf die Barrikade zu, schwang ihr Schwert und griff die verbliebene Priesterin an, die blutend zu Boden fiel."

# game/episode011.rpy:906
translate german ep011_rahia_talk_872e1264:

    # "Having cleared the way, we ran towards Eva's quarters."
    "Nachdem wir den Weg freigemacht hatten, rannten wir zu Evas Quartier."

# game/episode011.rpy:907
translate german ep011_rahia_talk_348d185a:

    # "We encountered no other hostiles and reached the Queen's wing without incident."
    "Wir sind auf keine weiteren Feinde gestoßen und haben den Flügel der Königin ohne Zwischenfälle erreicht."

# game/episode011.rpy:908
translate german ep011_rahia_talk_e9b23f86:

    # "When we neared Eva's chambers, sounds of struggle could be heard."
    "Als wir uns Evas Gemächern näherten, waren Kampfgeräusche zu hören."

# game/episode011.rpy:910
translate german ep011_rahia_talk_8bab300c:

    # rah "Careful."
    rah "Sei vorsichtig."

# game/episode011.rpy:911
translate german ep011_rahia_talk_2f4452a8:

    # "We turned a corner and found the entrance to the Queen's apartment besieged by several Acarhyn priestesses."
    "Wir bogen um eine Ecke und fanden den Eingang zum Appartement der Königin von mehreren Acarhyn-Priesterinnen belagert."

# game/episode011.rpy:912
translate german ep011_rahia_talk_5d2329f1:

    # "Their fire was returned by the Queen's guard who'd barricaded themselves."
    "Das Feuer wurde von der Leibgarde der Königin erwidert, die sich verbarrikadiert hatte."

# game/episode011.rpy:913
translate german ep011_rahia_talk_ef530212:

    # rah "I'll rush them, if you cover me."
    rah "Ich werde sie angreifen, wenn du mir Deckung gibst."

# game/episode011.rpy:914
translate german ep011_rahia_talk_a6994bef:

    # "I merely nodded at her whispered instructions."
    "Ich nickte lediglich zu ihren geflüsterten Anweisungen."

# game/episode011.rpy:915
translate german ep011_rahia_talk_4ba9b2a6:

    # rah "On my mark."
    rah "Auf mein Zeichen."

# game/episode011.rpy:917
translate german ep011_rahia_talk_e25df406:

    # "Rahia waved and darted forward, her sword at the ready."
    "Rahia winkte und stürmte vorwärts, das Schwert gezückt."

# game/episode011.rpy:918
translate german ep011_rahia_talk_73b2406c:

    # "As soon as she started running I fired upon the women who were trying to get inside Eva's room."
    "Sobald sie losrannte, eröffnete ich das Feuer auf die Frauen, die versuchten, in Evas Zimmer einzudringen."

# game/episode011.rpy:919
translate german ep011_rahia_talk_c9343748:

    # "One of the priestesses immediately collapsed, having received the brunt of my ranged attack."
    "Eine der Priesterinnen brach sofort zusammen, nachdem sie die volle Wucht meines Fernangriffs abbekommen hatte."

# game/episode011.rpy:921
translate german ep011_rahia_talk_be31719c:

    # "Rahia danced through the makeshift barricades, her blade whirling."
    "Rahia tanzte durch die provisorischen Barrikaden, ihre Klinge wirbelte."

# game/episode011.rpy:922
translate german ep011_rahia_talk_834eb4e2:

    # "In quick succession, three Acarhyn fell to the ground, killed by well-aimed strikes of the commander's sword."
    "In rascher Folge fielen drei Acarhyn zu Boden, getötet durch gezielte Hiebe des Kommandantenschwertes."

# game/episode011.rpy:923
translate german ep011_rahia_talk_deddf746:

    # "As Rahia spun towards her next target, one of the priestesses leveled her staff against her and took aim."
    "Als Rahia sich zu ihrem nächsten Ziel umdrehte, richtete eine der Priesterinnen ihren Stab gegen sie und zielte."

# game/episode011.rpy:925
translate german ep011_rahia_talk_e2140684:

    # "Before she could harm Rahia, I battered her with a volley of energy bolts."
    "Bevor sie Rahia etwas antun konnte, habe ich sie mit einer Salve von Energieblitzen attackiert."

# game/episode011.rpy:926
translate german ep011_rahia_talk_630958a0:

    # "The commander looked at me and grinned."
    "Der Kommandant sah mich an und grinste."

# game/episode011.rpy:928
translate german ep011_rahia_talk_7d413379:

    # rah "Commander Rahia and [p_name] to see the Queen."
    rah "Kommandant Rahia und [p_name], um die Königin zu sehen."

# game/episode011.rpy:929
translate german ep011_rahia_talk_582b9cad:

    # "We walked inside the Queen's chambers and were greeted by several grim-faced Acarhyn warriors."
    "Wir betraten die Gemächer der Königin und wurden von mehreren grimmig dreinblickenden Acarhyn-Kriegern empfangen."

# game/episode011.rpy:930
translate german ep011_rahia_talk_6e3a0b54:

    # "Rahia immediately demanded a sitrep while I looked around nervously for Eva."
    "Rahia verlangte sofort einen Lagebericht, während ich nervös nach Eva Ausschau hielt."

# game/episode011.rpy:932
translate german ep011_rahia_talk_0ebc0942:

    # "As soon as I spotted her she ran towards me and gripped me in a tight embrace."
    "Sobald ich sie erblickte, rannte sie auf mich zu und umarmte mich fest."

# game/episode011.rpy:933
translate german ep011_rahia_talk_5d932dc1:

    # c "I'm so glad you're okay."
    c "Ich bin so froh, dass es dir gut geht."

# game/episode011.rpy:935
translate german ep011_rahia_talk_17d5e119:

    # e "My guards were discussing how to break the siege, before you showed up."
    e "Meine Wachen berieten gerade darüber, wie man die Belagerung durchbrechen könnte, als du auftauchtest."

# game/episode011.rpy:936
translate german ep011_rahia_talk_390b7ea3:

    # e "I'm glad they didn't have to go through with their plan, because it would have been a bloodbath."
    e "Ich bin froh, dass sie ihren Plan nicht in die Tat umsetzen mussten, denn das wäre ein Blutbad gewesen."

# game/episode011.rpy:937
translate german ep011_rahia_talk_da835ea7:

    # c "It's a bit of a one-sided bloodbath right now after Rahia swept through."
    c "Im Moment herrscht hier ein ziemlich einseitiges Blutbad, nachdem Rahia so kräftig durchgezogen ist."

# game/episode011.rpy:939
translate german ep011_rahia_talk_aada826f:

    # e "Gruesome business."
    e "Eine grausame Angelegenheit."

# game/episode011.rpy:940
translate german ep011_rahia_talk_fc3c6538:

    # c "Erylin and the priest caste seem a little too well prepared for all this."
    c "Erylin und die Priesterkaste scheinen für all das etwas zu gut vorbereitet zu sein."

# game/episode011.rpy:941
translate german ep011_rahia_talk_16307e45:

    # e "Yes, they likely planned for a coup like this."
    e "Ja, sie haben wahrscheinlich einen solchen Putsch geplant."

# game/episode011.rpy:943
translate german ep011_rahia_talk_edb11c55:

    # e "I do wonder what pushed her over the edge."
    e "Ich frage mich wirklich, was sie zum Äußersten getrieben hat."

# game/episode011.rpy:944
translate german ep011_rahia_talk_1842e30e:

    # c "The Confessor, probably?"
    c "Die Bekennerin, vermutlich?"

# game/episode011.rpy:945
translate german ep011_rahia_talk_74db1a7f:

    # e "Yes..."
    e "Ja..."

# game/episode011.rpy:947
translate german ep011_rahia_talk_9c41e1df:

    # rah "Your Majesty, if I may interrupt."
    rah "Eure Majestät, wenn ich unterbrechen darf."

# game/episode011.rpy:949
translate german ep011_rahia_talk_01a19b12:

    # e "Yes, of course."
    e "Ja, natürlich."

# game/episode011.rpy:951
translate german ep011_rahia_talk_f871c818:

    # rah "We've lost all contact with the warrior delegation we sent along with Erylin."
    rah "Wir haben jeglichen Kontakt zu der Kriegerdelegation verloren, die wir zusammen mit Erylin entsandt haben."

# game/episode011.rpy:952
translate german ep011_rahia_talk_e7552b26:

    # rah "At the moment we still hold the west wing of the Citadel, but the priest caste is trying to get a foothold there as well."
    rah "Im Moment halten wir noch den Westflügel der Zitadelle, aber die Priesterkaste versucht auch dort Fuß zu fassen."

# game/episode011.rpy:953
translate german ep011_rahia_talk_38a4a2cb:

    # rah "Several of the major strongholds throughout Erigone have failed to report in."
    rah "Mehrere der wichtigsten Stützpunkte in ganz Erigone haben sich nicht gemeldet."

# game/episode011.rpy:954
translate german ep011_rahia_talk_d921c40f:

    # rah "It seems likely they're suffering the same attacks as we do in the Citadel."
    rah "Es sieht so aus, als würden sie in der Zitadelle die gleichen Angriffe erleiden wie wir."

# game/episode011.rpy:955
translate german ep011_rahia_talk_95112b57:

    # rah "We could try to hold our grasp on the Citadel, but considering Erylin is making her way back together with that intruder, it seems a lost cause."
    rah "Wir könnten versuchen, die Zitadelle zu halten, aber da Erylin zusammen mit diesem Eindringling zurückkehrt, scheint das aussichtslos."

# game/episode011.rpy:956
translate german ep011_rahia_talk_fe41ec99:

    # rah "My advice would be to head out into the jungle and regroup."
    rah "Mein Rat wäre, dich in den Dschungel zu begeben und dich neu zu sammeln."

# game/episode011.rpy:957
translate german ep011_rahia_talk_85e0bddd:

    # rah "The jungle would give us a tactical advantage while we conduct a campaign of guerrilla warfare and-"
    rah "Der Dschungel würde uns einen taktischen Vorteil verschaffen, während wir einen Guerillakrieg führen und-"

# game/episode011.rpy:959
translate german ep011_rahia_talk_855b321d:

    # e "What about our spaceports?"
    e "Und was ist mit unseren Weltraumbahnhöfen?"

# game/episode011.rpy:961
translate german ep011_rahia_talk_269d330d:

    # rah "Your Majesty?"
    rah "Eure Majestät?"

# game/episode011.rpy:963
translate german ep011_rahia_talk_7a50b885:

    # e "I'm not going to hide in the jungle while both Erylin and her new allies root us out."
    e "Ich werde mich nicht im Dschungel verstecken, während Erylin und ihre neuen Verbündeten uns aufspüren."

# game/episode011.rpy:964
translate german ep011_rahia_talk_926cb6de:

    # e "Do we have any means of leaving Erigone?"
    e "Gibt es irgendeine Möglichkeit, Erigone zu verlassen?"

# game/episode011.rpy:966
translate german ep011_rahia_talk_c93aca65:

    # rah "But, Your Majesty!"
    rah "Aber Eure Majestät!"

# game/episode011.rpy:968
translate german ep011_rahia_talk_7382d802:

    # e "Just answer the question."
    e "Beantworte einfach die Frage."

# game/episode011.rpy:970
translate german ep011_rahia_talk_66573888:

    # rah "All our spaceports are likely under attack, no contact has been possible for several hours."
    rah "Wahrscheinlich werden alle unsere Weltraumbahnhöfe angegriffen, seit mehreren Stunden ist kein Kontakt möglich."

# game/episode011.rpy:971
translate german ep011_rahia_talk_1aea3d4e:

    # rah "Only Aegisthus Shipyards is still responding to our hails, probably because of the small number of priests there."
    rah "Nur die Aegisthos-Werft antwortet noch auf unsere Anrufe, wahrscheinlich wegen der geringen Anzahl von Priestern dort."

# game/episode011.rpy:973
translate german ep011_rahia_talk_1a0dd852:

    # e "Then that's our destination."
    e "Dann ist das unser Ziel."

# game/episode011.rpy:974
translate german ep011_rahia_talk_334347f1:

    # e "Staying out of the hands of Erylin and the enemies she's collaborating with is of utmost importance."
    e "Es ist von größter Wichtigkeit, dass du dich nicht in die Hände von Erylin und ihren Feinden begibst, mit denen sie zusammenarbeitet."

# game/episode011.rpy:976
translate german ep011_rahia_talk_37e376f7:

    # rah "Of course, Your Majesty, I'll arrange for an escort immediately."
    rah "Selbstverständlich, Eure Majestät, ich werde sofort für eine Eskorte sorgen."

# game/episode011.rpy:979
translate german ep011_rahia_talk_9e7b324f:

    # e "My brother and his crew will also be accompanying me."
    e "Mein Bruder und seine Crew werden mich ebenfalls begleiten."

# game/episode011.rpy:981
translate german ep011_rahia_talk_f6ab68b1:

    # e "[p_name] and his crew will also be accompanying me."
    e "[p_name] und seine Crew werden mich ebenfalls begleiten."

# game/episode011.rpy:983
translate german ep011_rahia_talk_d324f40b:

    # rah "Of course, Your Majesty."
    rah "Selbstverständlich, Eure Majestät."

# game/episode011.rpy:984
translate german ep011_rahia_talk_7655b77e:

    # rah "What of the traitor currently imprisoned in our dungeons?"
    rah "Was ist mit dem Verräter, der derzeit in unseren Verliesen eingesperrt ist?"

# game/episode011.rpy:986
translate german ep011_rahia_talk_b6b71ff5:

    # e "We take her with us, she might know more of the threat we're currently facing."
    e "Wir nehmen sie mit, vielleicht weiß sie mehr über die Bedrohung, der wir uns derzeit gegenübersehen."

# game/episode011.rpy:987
translate german ep011_rahia_talk_20e98508:

    # c "I'm going to check on everyone, see if they're okay."
    c "Ich werde nach allen sehen und schauen, ob alles in Ordnung ist."

# game/episode011.rpy:989
translate german ep011_rahia_talk_b920cc0d:

    # rah "Two of my warriors will accompany you."
    rah "Zwei meiner Krieger werden dich begleiten."

# game/episode011.rpy:990
translate german ep011_rahia_talk_d6d6fc32:

    # rah "The transport to Aegisthus will leave in half an hour, make sure you're on time."
    rah "Der Transport nach Aigisthos fährt in einer halben Stunde ab, achte bitte darauf, pünktlich zu sein."

# game/episode011.rpy:992
translate german ep011_rahia_talk_59927281:

    # c "And Vess?"
    c "Und Vess?"

# game/episode011.rpy:993
translate german ep011_rahia_talk_c6aff31c:

    # rah "The traitor will be retrieved from the dungeons momentarily."
    rah "Der Verräter wird in Kürze aus den Verliesen geholt."

# game/episode011.rpy:995
translate german ep011_rahia_talk_18e44677:

    # e "Stay safe, [p_name_short]."
    e "Pass auf dich auf, [p_name_short]."

# game/episode011.rpy:996
translate german ep011_rahia_talk_255ecca1:

    # c "I will."
    c "Das werde ich."

# game/episode011.rpy:998
translate german ep011_rahia_talk_fc55a6ad:

    # "Together with my escort I began my search for my companions."
    "Gemeinsam mit meiner Begleitung begann ich meine Suche nach meinen Gefährtinnen."

# game/episode011.rpy:999
translate german ep011_rahia_talk_4c9b09c1:

    # "According to Rahia the west wing was considered relatively safe, so I focused my efforts there first."
    "Laut Rahia galt der Westflügel als relativ sicher, deshalb konzentrierte ich meine Bemühungen zunächst darauf."

# game/episode011.rpy:1000
translate german ep011_rahia_talk_0fbcbcae:

    # "Luckily, I didn't have to wander around long, as I was greeted by familiar faces as soon as I reached the dining hall."
    "Zum Glück musste ich nicht lange umherirren, denn sobald ich den Speisesaal erreichte, wurde ich von vertrauten Gesichtern begrüßt."

# game/episode011.rpy:1002
translate german ep011_rahia_talk_28afa06d:

    # av "Everything okay, [p_name_short]?"
    av "Alles in Ordnung, [p_name_short]?"

# game/episode011.rpy:1003
translate german ep011_rahia_talk_950f9150:

    # l "Have you spoken to Eva, is she okay?"
    l "Hast du mit Eva gesprochen? Geht es ihr gut?"

# game/episode011.rpy:1004
translate german ep011_rahia_talk_da74b818:

    # c "She's under the protection of the warrior caste."
    c "Sie steht unter dem Schutz der Kriegerkaste."

# game/episode011.rpy:1005
translate german ep011_rahia_talk_157882fc:

    # c "The Acarhyn seem to be undergoing a change in leadership."
    c "Bei den Acarhyn scheint es einen Führungswechsel zu geben."

# game/episode011.rpy:1007
translate german ep011_rahia_talk_01805588:

    # ki "So we noticed, the priests are taking over?"
    ki "Uns ist aufgefallen, dass die Priester die Macht übernehmen?"

# game/episode011.rpy:1008
translate german ep011_rahia_talk_fa6b2c91:

    # c "Yes, Erylin seems to have found a reason to finally show her hand."
    c "Ja, Erylin scheint einen Grund gefunden zu haben, endlich ihre Karten aufzudecken."

# game/episode011.rpy:1009
translate german ep011_rahia_talk_e2a79040:

    # ki "Great, I've never witnessed a violent overthrow from up close before..."
    ki "Toll, ich habe noch nie einen gewaltsamen Umsturz aus nächster Nähe miterlebt..."

# game/episode011.rpy:1010
translate german ep011_rahia_talk_8aad257d:

    # ki "Though now that I have, I'm not sure I'm all that interested anymore."
    ki "Jetzt, wo ich es habe, bin ich mir nicht mehr so ​​sicher, ob es mich überhaupt noch interessiert."

# game/episode011.rpy:1011
translate german ep011_rahia_talk_51402c89:

    # c "The warrior caste has most of the west wing of the Citadel under control, but it's difficult to say how long that will last."
    c "Die Kriegerkaste hat den größten Teil des Westflügels der Zitadelle unter ihrer Kontrolle, aber es ist schwer zu sagen, wie lange das anhalten wird."

# game/episode011.rpy:1012
translate german ep011_rahia_talk_b89ea2d4:

    # c "We are to meet with Eva in half an hour."
    c "Wir treffen uns in einer halben Stunde mit Eva."

# game/episode011.rpy:1013
translate german ep011_rahia_talk_da47691f:

    # c "Together we'll drive towards Aegisthus Shipyards and hopefully leave the planet."
    c "Gemeinsam fahren wir Richtung Aegishus Shipyards und werden hoffentlich den Planeten verlassen."

# game/episode011.rpy:1015
translate german ep011_rahia_talk_68730f26:

    # l "Eva is coming with us?"
    l "Kommt Eva mit uns?"

# game/episode011.rpy:1016
translate german ep011_rahia_talk_6e920e78:

    # c "That's the plan, though I still need to convince Rahia..."
    c "Das ist der Plan, obwohl ich Rahia noch überzeugen muss..."

# game/episode011.rpy:1018
translate german ep011_rahia_talk_211fdf48:

    # ce "Is the Bastard spaceworthy again?"
    ce "Ist der Bastard wieder weltraumtauglich?"

# game/episode011.rpy:1020
translate german ep011_rahia_talk_4efc6e35:

    # th "Around the time I left the shipyard, when the repairs were halted, they were working on the ship's interior."
    th "Ungefähr zu der Zeit, als ich die Werft verließ und die Reparaturen eingestellt wurden, arbeiteten sie am Innenausbau des Schiffes."

# game/episode011.rpy:1021
translate german ep011_rahia_talk_a7cd592a:

    # th "All systems had already been revised and repaired."
    th "Alle Systeme wurden bereits überarbeitet und repariert."

# game/episode011.rpy:1022
translate german ep011_rahia_talk_1ea8ab4c:

    # th "So I do believe she's spaceworthy."
    th "Also ich glaube schon, dass sie weltraumtauglich ist."

# game/episode011.rpy:1023
translate german ep011_rahia_talk_e91de0ef:

    # c "Work on the ship has resumed as well, so all the essential repairs may have been completed."
    c "Die Arbeiten am Schiff wurden ebenfalls wieder aufgenommen, sodass alle notwendigen Reparaturen möglicherweise abgeschlossen sind."

# game/episode011.rpy:1024
translate german ep011_rahia_talk_9658ee05:

    # c "But let's not dawdle and meet up with Eva."
    c "Aber lasst uns nicht trödeln und uns mit Eva treffen."

# game/episode011.rpy:1025
translate german ep011_rahia_talk_f5d9a815:

    # ra "What about Vess?"
    ra "Und was ist mit Vess?"

# game/episode011.rpy:1026
translate german ep011_rahia_talk_31fb4e60:

    # c "Vess remains an Acarhyn prisoner, but will accompany us to the shipyards."
    c "Vess bleibt Gefangene von Acarhyn, wird uns aber zu den Werften begleiten."

# game/episode011.rpy:1028
translate german ep011_rahia_talk_db5d22e6:

    # "We hurried back towards Eva's quarters where a group of warriors awaited us."
    "Wir eilten zurück zu Evas Quartier, wo eine Gruppe Krieger auf uns wartete."

# game/episode011.rpy:1030
translate german ep011_rahia_talk_2a479b46:

    # "The platoon led us to the beast pens where several armored trucks were already waiting for us."
    "Der Zug führte uns zu den Tiergehegen, wo bereits mehrere gepanzerte Lastwagen auf uns warteten."

# game/episode011.rpy:1031
translate german ep011_rahia_talk_0a763b0b:

    # "Sounds of battle could clearly be heard here and the battlements of the Citadel were full of warriors trying to keep any priestess incursions at bay."
    "Die Geräusche des Kampfes waren hier deutlich zu hören, und die Zinnen der Zitadelle waren voller Krieger, die versuchten, jegliche Einfälle der Priesterinnen abzuwehren."

# game/episode011.rpy:1033
translate german ep011_rahia_talk_908791b6:

    # "The Acarhyn made us board the three trucks hastily, checking the perimeter for any enemy incursions."
    "Der Acarhyn ließ uns hastig in die drei Lastwagen einsteigen und überprüfte den Umkreis auf mögliche feindliche Eindringlinge."

# game/episode011.rpy:1034
translate german ep011_rahia_talk_2c2cc49b:

    # "Vess was treated as a particularly dangerous prisoner, which struck me as a little overblown, as she crawled inside her own personal prison transport."
    "Vess wurde als besonders gefährliche Gefangene behandelt, was mir etwas übertrieben erschien, als sie in ihren eigenen persönlichen Gefangenentransport kroch."

# game/episode011.rpy:1036
translate german ep011_rahia_talk_e6ab844a:

    # "The engines roared and the trucks quickly drove out of the beast pen."
    "Die Motoren heulten auf und die Lastwagen fuhren schnell aus dem Tiergehege."

# game/episode011.rpy:1037
translate german ep011_rahia_talk_e12ee421:

    # "Right when the trucks reached the first trees of the jungle, a group of priestesses broke through warrior ranks."
    "Genau als die Lastwagen die ersten Bäume des Dschungels erreichten, durchbrach eine Gruppe Priesterinnen die Reihen der Krieger."

# game/episode011.rpy:1039
translate german ep011_rahia_talk_a57c1025:

    # "The Acarhyn rained energy bolts on the trucks's armored plating, but were quickly beat down by an overwhelming force of warriors."
    "Die Acarhyn beschossen die Panzerplatten der Lastwagen mit Energieblitzen, wurden aber schnell von einer überwältigenden Streitmacht von Kriegern besiegt."

# game/episode011.rpy:1041
translate german ep011_rahia_talk_d2bf116a:

    # "There were no further assaults, even when we left the protection of the jungle's foliage and drove along a rocky mountain path."
    "Es gab keine weiteren Angriffe, auch nicht, als wir den Schutz des Dschungellaubes verließen und einen felsigen Bergpfad entlangfuhren."

# game/episode011.rpy:1043
translate german ep011_rahia_talk_70163a4e:

    # "Eva was sitting in the truck with us, having swapped her ornate robes for something more suitable for the ride ahead."
    "Eva saß mit uns im Lastwagen; sie hatte ihre prunkvollen Gewänder gegen etwas für die bevorstehende Fahrt Angemesseneres getauscht."

# game/episode011.rpy:1044
translate german ep011_rahia_talk_649ef0c6:

    # "Nobody spoke much, all ears intent on listening for any signs of trouble along the way."
    "Niemand sprach viel, alle Ohren waren darauf gerichtet, auf Anzeichen von Schwierigkeiten unterwegs zu lauschen."

# game/episode011.rpy:1046
translate german ep011_rahia_talk_0f42e41e:

    # "The roads were deserted however, as the fighting was mostly concentrated in the larger population centers."
    "Die Straßen waren jedoch wie ausgestorben, da sich die Kämpfe hauptsächlich auf die größeren Bevölkerungszentren konzentrierten."

# game/episode011.rpy:1047
translate german ep011_rahia_talk_7c4d25bf:

    # "I just hoped we would get to the shipyards in time, before anybody got wind of our intentions."
    "Ich hoffte nur, wir würden es rechtzeitig zur Werft schaffen, bevor irgendjemand von unseren Absichten Wind bekam."

# game/episode011.rpy:1048
translate german ep011_rahia_talk_3135776d:

    # "Unfortunately, as Erylin was anything but stupid, it would just be a matter of time before Aegisthus would be crawling with her minions."
    "Da Erylin leider alles andere als dumm war, würde es nur eine Frage der Zeit sein, bis Aegisthos von ihren Dienern überlaufen sein würde."

# game/episode011.rpy:1049
translate german ep011_rahia_talk_fcec9f7c:

    # "The Confessor forcing her hand could give us the edge we'd need to escape this planet relatively unscathed."
    "Wenn die Beichtmutter sie dazu zwingt, könnte uns das den entscheidenden Vorteil verschaffen, den wir brauchen, um diesen Planeten relativ unbeschadet zu verlassen."

# game/episode011.rpy:1050
translate german ep011_rahia_talk_791b36df:

    # "After a two-hour drive, our truck came to a screeching halt."
    "Nach zweistündiger Fahrt kam unser LKW mit quietschenden Reifen zum Stehen."

# game/episode011.rpy:1052
translate german ep011_rahia_talk_665ddf89:

    # ce "We haven't arrived yet, have we?"
    ce "Wir sind noch nicht angekommen, oder?"

# game/episode011.rpy:1053
translate german ep011_rahia_talk_5ae7977d:

    # "Her question was answered by the heated cries and sounds of enemy fire outside."
    "Ihre Frage wurde durch die hitzigen Schreie und das Draußenfeuer des Feindes beantwortet."

# game/episode011.rpy:1054
translate german ep011_rahia_talk_d18030d6:

    # "Two thundering crashes made many of us rise from our seats."
    "Zwei ohrenbetäubende Krachgeräusche ließen viele von uns von ihren Sitzen aufspringen."

# game/episode011.rpy:1056
translate german ep011_rahia_talk_da15dbda:

    # na "What's happening?"
    na "Was ist los?"

# game/episode011.rpy:1057
translate german ep011_rahia_talk_da77722c:

    # "I opened the entrance hatch to confirm the worst."
    "Ich öffnete die Eingangsluke, um das Schlimmste zu bestätigen."

# game/episode011.rpy:1059
translate german ep011_rahia_talk_f5f8c411:

    # "The trucks were separated between a mess of rocks and boulders, deposited by an avalanche, no doubt man-made."
    "Die Lastwagen waren zwischen einem Haufen Felsen und Geröll verstreut, die von einer Lawine abgelagert worden waren, die zweifellos von Menschen verursacht worden war."

# game/episode011.rpy:1060
translate german ep011_rahia_talk_40a149cc:

    # "Our truck had been the lead vehicle, the one containing Vess trailing behind."
    "Unser Lastwagen war das Führungsfahrzeug, das Fahrzeug mit Vess folgte dahinter."

# game/episode011.rpy:1062
translate german ep011_rahia_talk_133c5ad2:

    # "The last vehicle was manned by Rahia and much of the warrior contingent and now suffered heavy fire from several priestesses."
    "Das letzte Fahrzeug war mit Rahia und einem Großteil der Kriegereinheit besetzt und geriet nun unter heftiges Feuer mehrerer Priesterinnen."

# game/episode011.rpy:1064
translate german ep011_rahia_talk_ded303ce:

    # c "Ambush!"
    c "Hinterhalt!"

# game/episode011.rpy:1065
translate german ep011_rahia_talk_8e70f145:

    # "I closed the hatch before one of the priestesses could take aim at our vehicle."
    "Ich schloss die Luke, bevor eine der Priesterinnen auf unser Fahrzeug zielen konnte."

# game/episode011.rpy:1066
translate german ep011_rahia_talk_38ff1d9f:

    # "Eva didn't hesitate and radioed our driver."
    "Eva zögerte nicht und funkte unseren Fahrer an."

# game/episode011.rpy:1068
translate german ep011_rahia_talk_0213f060:

    # e "There's nothing we can do for the warriors behind us!{w} Drive!"
    e "Für die Krieger hinter uns können wir nichts tun! {w} Fahr!"

# game/episode011.rpy:1069
translate german ep011_rahia_talk_8c31a5ad:

    # "There was a short pause on the other end of the line, but Eva seemed confident she would be obeyed."
    "Am anderen Ende der Leitung entstand eine kurze Pause, aber Eva schien zuversichtlich, dass man ihr gehorchen würde."

# game/episode011.rpy:1071
translate german ep011_rahia_talk_e6e82f61:

    # woman "As you wish, Your Majesty."
    woman "Wie Ihr wünscht, Eure Majestät."

# game/episode011.rpy:1072
translate german ep011_rahia_talk_ef03a5d3:

    # "Immediately after those words, the truck roared to life again and I felt its wheels turning."
    "Unmittelbar nach diesen Worten erwachte der Lastwagen wieder zum Leben und ich spürte, wie sich seine Räder drehten."

# game/episode011.rpy:1073
translate german ep011_rahia_talk_3bc1eac3:

    # "I could hear the other one starting up as well."
    "Ich konnte hören, wie der andere ebenfalls anlief."

# game/episode011.rpy:1075
translate german ep011_rahia_talk_0c79e839:

    # "Several projectiles hit the hull of the vehicle, but did nothing to stop us from accelerating towards our destination."
    "Mehrere Geschosse trafen die Fahrzeughülle, konnten uns aber nicht daran hindern, weiter in Richtung unseres Ziels zu beschleunigen."

# game/episode011.rpy:1079
translate german ep011_rahia_talk_08ac1f06:

    # "The road ahead held no further nasty surprises and we rolled past the heavily guarded entrance of the shipyards after two hours."
    "Die vor uns liegende Strecke hielt keine weiteren bösen Überraschungen bereit, und nach zwei Stunden passierten wir den schwer bewachten Eingang der Werft."

# game/episode011.rpy:1081
translate german ep011_rahia_talk_ad45774e:

    # "The place was crawling with Acarhyn warriors, their apprehension of possibly imminent violence clearly noticeable."
    "Der Ort wimmelte von Acarhyn-Kriegern, deren Befürchtung einer möglicherweise unmittelbar bevorstehenden Gewalttat deutlich zu erkennen war."

# game/episode011.rpy:1082
translate german ep011_rahia_talk_859e6a55:

    # "As soon as Eva exited the truck, all Acarhyn jumped to attention and saluted their Queen."
    "Sobald Eva aus dem Lastwagen stieg, sprangen alle Acarhyn in Habachtstellung und salutierten ihrer Königin."

# game/episode011.rpy:1084
translate german ep011_rahia_talk_807657f8:

    # e "Please escort us to the Iron Bastard."
    e "Bitte begleite uns zum Iron Bastard."

# game/episode011.rpy:1085
translate german ep011_rahia_talk_f2c7f117:

    # woman "But...{w} Your Majesty?"
    woman "Aber...{w} Eure Majestät?"

# game/episode011.rpy:1087
translate german ep011_rahia_talk_e2dce9a0:

    # e "I will leave this planet on my brother's ship and become Virgin Queen In Exile, until the situation here on Erigone is under control again."
    e "Ich werde diesen Planeten auf dem Schiff meines Bruders verlassen und zur Jungfräulichen Königin im Exil werden, bis die Situation hier auf Erigone wieder unter Kontrolle ist."

# game/episode011.rpy:1089
translate german ep011_rahia_talk_ee7bf293:

    # e "I will leave this planet on my friend's ship and become Virgin Queen In Exile, until the situation here on Erigone is under control again."
    e "Ich werde diesen Planeten auf dem Schiff meines Freundes verlassen und zur Jungfräulichen Königin im Exil werden, bis die Situation hier auf Erigone wieder unter Kontrolle ist."

# game/episode011.rpy:1090
translate german ep011_rahia_talk_a93bf6f7:

    # "Nobody dared to protest her, possibly because no high-ranking Acarhyn were present to hear Eva's request."
    "Niemand wagte es, ihr zu widersprechen, möglicherweise weil keine hochrangigen Acarhyn anwesend waren, um Evas Bitte zu hören."

# game/episode011.rpy:1091
translate german ep011_rahia_talk_84931c99:

    # "I'm sure many of the warriors balked at the thought of their monarch leaving their planet in a time of crisis."
    "Ich bin mir sicher, dass viele der Krieger bei dem Gedanken, dass ihr Monarch ihren Planeten in einer Zeit der Krise verlassen könnte, zurückschreckten."

# game/episode011.rpy:1092
translate german ep011_rahia_talk_2a9332f1:

    # "Since Eva didn't exactly choose to become Virgin Queen, I didn't begrudge her the urge to get the hell out of here."
    "Da Eva sich nicht gerade selbst ausgesucht hatte, Jungfrauenkönigin zu werden, konnte ich ihr den Drang, hier schleunigst zu verschwinden, nicht verdenken."

# game/episode011.rpy:1093
translate german ep011_rahia_talk_4795fc52:

    # "The Acarhyn who spoke up to my sister cleared her throat and bowed."
    "Die Acarhyn, die zu meiner Schwester gesprochen hatte, räusperte sich und verbeugte sich."

# game/episode011.rpy:1095
translate german ep011_rahia_talk_f745a434:

    # woman "Follow me, My Queen."
    woman "Folge mir, meine Königin."

# game/episode011.rpy:1097
translate german ep011_rahia_talk_566791b9:

    # "The Acarhyn warrior walked down a large hallway and we followed suit."
    "Der Krieger von Acarhyn schritt einen langen Flur entlang, und wir folgten ihm."

# game/episode011.rpy:1098
translate german ep011_rahia_talk_f4450fef:

    # "I looked back and saw that Vess was marched down the same hallway too."
    "Ich blickte zurück und sah, dass auch Vess denselben Flur entlanggeführt wurde."

# game/episode011.rpy:1100
translate german ep011_rahia_talk_d9ce75ea:

    # "The warrior paused near a massive door, before it opened with a metallic groan."
    "Der Krieger blieb vor einer massiven Tür stehen, bevor sie sich mit einem metallischen Stöhnen öffnete."

# game/episode011.rpy:1102
translate german ep011_rahia_talk_1596a8fe:

    # "The hangar was occupied by our ship, completely restored into its former crummy glory."
    "Der Hangar wurde von unserem Schiff belegt, das vollständig in seinen früheren, etwas heruntergekommenen Glanz zurückversetzt wurde."

# game/episode011.rpy:1103
translate german ep011_rahia_talk_f43107be:

    # "It was the same ship, save for some subtle changes."
    "Es war dasselbe Schiff, abgesehen von einigen kleinen Änderungen."

# game/episode011.rpy:1104
translate german ep011_rahia_talk_3871c4df:

    # "Overall the hull looked much sturdier, its lines less craggy than before."
    "Insgesamt wirkte der Rumpf deutlich robuster, seine Linienführung weniger kantig als zuvor."

# game/episode011.rpy:1105
translate german ep011_rahia_talk_4763b758:

    # "From what I understood, most work was done on the inside and I couldn't wait to step foot in the old bucket again."
    "Soweit ich das verstanden habe, wurden die meisten Arbeiten im Inneren durchgeführt, und ich konnte es kaum erwarten, wieder in den alten Eimer zu steigen."

# game/episode011.rpy:1107
translate german ep011_rahia_talk_def9d1a5:

    # e "Prepare this ship for take-off."
    e "Bereite dieses Schiff für den Start vor."

# game/episode011.rpy:1108
translate german ep011_rahia_talk_2b7d9277:

    # e "Once Commander Rahia arrives, tell her that the crown appreciates her efforts to resecure Erigone and that I will return to Erigone as soon as the time is ripe."
    e "Sobald Kommandantin Rahia eintrifft, richte ihr aus, dass die Krone ihre Bemühungen um die Rückeroberung von Erigone schätzt und dass ich nach Erigone zurückkehren werde, sobald die Zeit reif ist."

# game/episode011.rpy:1109
translate german ep011_rahia_talk_295c18cc:

    # "I thought that Eva's last statement was deliciously vague, but the Acarhyn warrior swallowed it nonetheless."
    "Ich fand Evas letzte Aussage herrlich vage, aber der Acarhyn-Krieger schluckte sie trotzdem."

# game/episode011.rpy:1111
translate german ep011_rahia_talk_13d8492f:

    # woman "What about the prisoner, Your Majesty?"
    woman "Was ist mit dem Gefangenen, Majestät?"

# game/episode011.rpy:1113
translate german ep011_rahia_talk_d970a543:

    # e "Yes, [p_name_short], what should we do about her?"
    e "Ja, [p_name_short], was sollen wir mit ihr machen?"

# game/episode011.rpy:1118
translate german ep011_rahia_talk_4f517d2a:

    # c "I don't really care what happens to her, just leave her here."
    c "Mir ist es eigentlich egal, was mit ihr passiert, lass sie einfach hier."

# game/episode011.rpy:1119
translate german ep011_rahia_talk_ade84f6a:

    # ve "No!{w} Please!{w} They'll kill me!"
    ve "Nein!{w} Bitte!{w} Sie werden mich töten!"

# game/episode011.rpy:1120
translate german ep011_rahia_talk_80019bed:

    # "Eva looked at me expectantly and Vess used that silence to wrestle herself forwards, demanding our attention."
    "Eva blickte mich erwartungsvoll an, und Vess nutzte diese Stille, um sich nach vorne zu drängen und unsere Aufmerksamkeit zu fordern."

# game/episode011.rpy:1122
translate german ep011_rahia_talk_fdc8a849:

    # ve "Please, take me with you!"
    ve "Bitte, nimm mich mit!"

# game/episode011.rpy:1123
translate german ep011_rahia_talk_6cae4579:

    # ve "I know things, about the Confessor...{w} about Eva!"
    ve "Ich weiß Dinge über den Beichtvater...{w} über Eva!"

# game/episode011.rpy:1125
translate german ep011_rahia_talk_90786515:

    # c "What do you know about my sister?!"
    c "Was weißt du über meine Schwester?!"

# game/episode011.rpy:1127
translate german ep011_rahia_talk_37f45e24:

    # c "What do you know about Eva?!"
    c "Was weißt du über Eva?!"

# game/episode011.rpy:1128
translate german ep011_rahia_talk_64d12a46:

    # ve "Please take me with you and I'll tell you.{w} Please!"
    ve "Bitte nimm mich mit, dann erzähle ich es dir. {w} Bitte!"

# game/episode011.rpy:1129
translate german ep011_rahia_talk_6e435e28:

    # "Getting the information out of her in the hangar seemed a little pointless, considering we were on a strict timetable."
    "Es schien etwas sinnlos, ihr die Informationen im Hangar zu entlocken, da wir unter großem Zeitdruck standen."

# game/episode011.rpy:1130
translate german ep011_rahia_talk_06601cb7:

    # c "Fine, load her on the Bastard."
    c "Gut, lade sie auf den Bastard."

# game/episode011.rpy:1131
translate german ep011_rahia_talk_3e0b8ba1:

    # c "Unless it was demolished during the repairs, the Iron Bastard has a brig where she can remain as long as she's useful."
    c "Sofern sie nicht während der Reparaturarbeiten abgerissen wurde, hat die Eiserne Bastard eine Arrestzelle, in der sie so lange bleiben kann, wie sie nützlich ist."

# game/episode011.rpy:1133
translate german ep011_rahia_talk_956ee68d:

    # ve "Thank you, [p_name]!"
    ve "Danke, [p_name]!"

# game/episode011.rpy:1135
translate german ep011_rahia_talk_f8d81fe2:

    # c "Vess knows things about the Confessor, any further information she could give us would be valuable."
    c "Vess weiß einiges über den Bekenner, jede weitere Information, die sie uns geben könnte, wäre wertvoll."

# game/episode011.rpy:1136
translate german ep011_rahia_talk_6bae079a:

    # c "Unless it was demolished during the repairs, the Iron Bastard has a brig where she can remain for the time being."
    c "Sofern sie nicht während der Reparaturarbeiten abgerissen wurde, hat die Iron Bastard eine Arrestzelle, in der sie vorerst bleiben kann."

# game/episode011.rpy:1138
translate german ep011_rahia_talk_2d916c75:

    # "The Acarhyn saluted their Virgin Queen once more as she entered the Iron Bastard."
    "Die Acarhyn grüßten ihre Jungfräuliche Königin ein letztes Mal, als sie den Eisernen Bastard betrat."

# game/episode011.rpy:1139
translate german ep011_rahia_talk_e95bd77f:

    # "Céline hurried to the bridge to take the pilot seat again."
    "Céline eilte zur Brücke, um wieder den Pilotensitz einzunehmen."

# game/episode011.rpy:1140
translate german ep011_rahia_talk_814c7fb5:

    # "The hangar bay was cleared as soon as she started up the engines of our ship."
    "Die Hangarbucht wurde geräumt, sobald sie die Motoren unseres Schiffes gestartet hatte."

# game/episode011.rpy:1142
translate german ep011_rahia_talk_124b5a21:

    # "The exterior doors were opening and we got the all clear from flight control."
    "Die Außentüren öffneten sich und wir erhielten von der Flugleitung die Freigabe."

# game/episode011.rpy:1143
translate german ep011_rahia_talk_50ac7f97:

    # "I sincerely hoped the ground-to-air defenses weren't recalibrated to destroy any outbound ship..."
    "Ich hoffte inständig, dass die Boden-Luft-Abwehrsysteme nicht so umkalibriert worden waren, dass sie jedes auslaufende Schiff zerstören würden..."

# game/episode011.rpy:1144
translate german ep011_rahia_talk_d5f71c15:

    # "Take-off was smooth and we began our climb towards the edge of the planet's atmosphere."
    "Der Start verlief reibungslos und wir begannen unseren Aufstieg zum Rand der Erdatmosphäre."

# game/episode011.rpy:1146
translate german ep011_rahia_talk_7f8481fa:

    # "On the ground below us, a familiar truck arrived at the shipyards."
    "Unten unter uns traf ein bekannter Lastwagen auf dem Werftgelände ein."

# game/episode011.rpy:1148
translate german ep011_rahia_talk_344b3934:

    # "Part of me was glad to know Rahia might have made it out of that ambush."
    "Ein Teil von mir war froh zu wissen, dass Rahia dem Hinterhalt möglicherweise entkommen war."

# game/episode011.rpy:1150
translate german ep011_rahia_talk_b895c6f4:

    # "Rahia might have made it out of that ambush after all."
    "Rahia hat den Hinterhalt vielleicht doch überlebt."

# game/episode011.rpy:1151
translate german ep011_rahia_talk_4fc99ed8:

    # "I could only guess her reaction when she found out about the Queen's exile."
    "Ich konnte ihre Reaktion nur erahnen, als sie von der Verbannung der Königin erfuhr."

# game/episode011.rpy:1152
translate german ep011_rahia_talk_4ec0e5e1:

    # "Our sensors were mercifully quiet as no threats appeared during our ascent."
    "Unsere Sensoren blieben glücklicherweise ruhig, da während unseres Aufstiegs keine Bedrohungen auftraten."

# game/episode011.rpy:1154
translate german ep011_rahia_talk_cf4e213c:

    # "We broke atmosphere moments later and I'm sure I wasn't the only one to breathe a sigh of relief."
    "Wir haben die Atmosphäre nur Augenblicke später durchbrochen und ich bin sicher, ich war nicht der Einzige, der erleichtert aufatmete."

# game/episode011.rpy:1155
translate german ep011_rahia_talk_b5772f38:

    # c "Right, Céline, take us as far away as possible from this place."
    c "Richtig, Céline, bring uns so weit wie möglich von diesem Ort weg."

# game/episode011.rpy:1156
translate german ep011_rahia_talk_309ce78d:

    # ce "With pleasure!"
    ce "Sehr gerne!"

# game/episode011.rpy:1158
translate german ep011_rahia_talk_68b12b9b:

    # "We left the Acarhyn's weird planetary system behind us with increasing speed."
    "Wir ließen das seltsame Planetensystem der Acarhyn mit zunehmender Geschwindigkeit hinter uns."

# game/episode011.rpy:1159
translate german ep011_rahia_talk_44953f7b:

    # "As we increased the distance between Erigone and the Iron Bastard, it seemed like a good idea to check out the improvements that were installed."
    "Als wir den Abstand zwischen Erigone und dem Iron Bastard vergrößerten, schien es eine gute Idee, die vorgenommenen Verbesserungen zu begutachten."

# game/episode011.rpy:1161
translate german ep011_rahia_talk_8cefca7a:

    # "I left Céline in charge of the Bastard and went exploring."
    "Ich überließ Céline die Aufsicht über den Bastard und ging auf Erkundungstour."

# game/episode011.rpy:1171
translate german ep011_station_cc93fdd9:

    # "Thyia’s description of Shuty Station turned out to be very apt."
    "Thyias Beschreibung von Shuty Station erwies sich als sehr treffend."

# game/episode011.rpy:1173
translate german ep011_station_e72599a1:

    # "Shuty Station looked like a big disappointment in every single way."
    "Shuty Station entpuppte sich in jeder Hinsicht als große Enttäuschung."

# game/episode011.rpy:1174
translate german ep011_station_6f76b64a:

    # "From the outside it looked like a derelict ball of junk and that same charm was evident in the interior of the station."
    "Von außen sah es aus wie ein verlassener Schrotthaufen, und genau diesen Charme zeigte sich auch im Inneren des Bahnhofs."

# game/episode011.rpy:1176
translate german ep011_station_30d18c65:

    # "Outside the docks there was a small communal marketplace where we stopped to get our bearings."
    "Außerhalb des Hafengeländes gab es einen kleinen Gemeinschaftsmarkt, wo wir anhielten, um uns zu orientieren."

# game/episode011.rpy:1179
translate german ep011_station_0e24bf33:

    # th "The place hasn’t changed much."
    th "Der Ort hat sich nicht groß verändert."

# game/episode011.rpy:1180
translate german ep011_station_5c6a2aee:

    # th "Everything is still full of no charm whatsoever."
    th "Alles ist immer noch völlig reizlos."

# game/episode011.rpy:1182
translate german ep011_station_5feee4e4:

    # "Aven and Céline agreed to split up, so that they could get some supplies, while Thyia and I haggled for fuel."
    "Aven und Céline beschlossen, sich aufzuteilen, um Vorräte zu besorgen, während Thyia und ich um Treibstoff feilschten."

# game/episode011.rpy:1184
translate german ep011_station_5b1e5995:

    # "Thyia found her mark in no time, and the way she shook down the hapless merchant could almost be called a work of art."
    "Thyia hatte ihr Ziel im Nu gefunden, und die Art und Weise, wie sie den unglückseligen Kaufmann abzockte, könnte man fast als Kunstwerk bezeichnen."

# game/episode011.rpy:1185
translate german ep011_station_4f185d27:

    # "When we headed back to the marketplace near the docks, I noticed she was very tense."
    "Als wir zum Marktplatz in der Nähe der Docks zurückgingen, bemerkte ich, dass sie sehr angespannt war."

# game/episode011.rpy:1187
translate german ep011_station_06dd35ee:

    # c "I’m not sure that fuel merchant will be able to feed his children."
    c "Ich bin mir nicht sicher, ob der Treibstoffhändler seine Kinder ernähren kann."

# game/episode011.rpy:1188
translate german ep011_station_69addd08:

    # th "You believed that story about all those mouths he had to feed?"
    th "Du hast die Geschichte von all den Mäulern, die er zu stopfen hatte, geglaubt?"

# game/episode011.rpy:1189
translate german ep011_station_3b305138:

    # c "Not entirely..."
    c "Nicht ganz..."

# game/episode011.rpy:1191
translate german ep011_station_c5bb0029:

    # th "Trallraghs are barren and their offspring are born in artificial pods replicating the natural conditions under which the first Trallragh was born."
    th "Trallraghs sind unfruchtbar und ihre Nachkommen werden in künstlichen Kapseln geboren, die die natürlichen Bedingungen nachbilden, unter denen der erste Trallragh geboren wurde."

# game/episode011.rpy:1192
translate german ep011_station_4c475a7b:

    # th "Any Trallragh with a sob story about their starving family is lying through their teeth."
    th "Jeder Trallragh, der eine rührselige Geschichte über seine hungernde Familie erzählt, lügt, dass sich die Balken biegen."

# game/episode011.rpy:1193
translate german ep011_station_b7f4b180:

    # c "Noted."
    c "Zur Kenntnis genommen."

# game/episode011.rpy:1197
translate german ep011_station_f477579b:

    # "Whatever had plagued Thyia wasn't evident in her demeanor after that little exchange."
    "Was auch immer Thyia bedrückt hatte, war nach diesem kurzen Wortwechsel in ihrem Verhalten nicht mehr zu erkennen."

# game/episode011.rpy:1199
translate german ep011_station_68a5e75c:

    # "When we returned to the marketplace near the docks, the others were already waiting."
    "Als wir zum Marktplatz in der Nähe der Docks zurückkehrten, warteten die anderen bereits."

# game/episode011.rpy:1200
translate german ep011_station_bfac0d67:

    # "Large screens were displaying announcements overhead and I didn’t pay much attention to the prattle of the commercials and general broadcast messages until I heard a familiar name."
    "Auf großen Bildschirmen über uns wurden Durchsagen angezeigt, und ich schenkte dem Geplapper der Werbespots und allgemeinen Durchsagen keine große Beachtung, bis ich einen vertrauten Namen hörte."

# game/episode011.rpy:1203
translate german ep011_station_8128b014:

    # man "...Church of Radiant Dawn of Verdigris V is enlisting the help of anybody who’ll come forward with information regarding the kidnapping of the son of His Holiness, Hierophant Sacleus."
    man "...Die Kirche der Strahlenden Morgenröte von Verdigris V bittet um die Hilfe aller, die Informationen über die Entführung des Sohnes Seiner Heiligkeit, Hierophant Sacleus, liefern können."

# game/episode011.rpy:1205
translate german ep011_station_106418d6:

    # man "Arion Sacleus is a five foot five tall, white-haired boy, likely travelling under the assumed name ‘Raene’."
    man "Arion Sacleus ist ein 1,65 Meter großer, weißhaariger Junge, der wahrscheinlich unter dem angenommenen Namen „Raene“ reist."

# game/episode011.rpy:1206
translate german ep011_station_fd1a9198:

    # man "With him are several known criminals, believed to be led by this man."
    man "Bei ihm befinden sich mehrere bekannte Kriminelle, die vermutlich von diesem Mann angeführt werden."

# game/episode011.rpy:1208
translate german ep011_station_953d748d:

    # man "[p_name] Valenmann de Lonval."
    man "[p_name] Valenmann de Lonval."

# game/episode011.rpy:1209
translate german ep011_station_8b474c3d:

    # man "Approach any of the members of his violent band of thugs with due caution."
    man "Nähere dich jedem Mitglied seiner gewalttätigen Bande von Schlägern mit gebotener Vorsicht."

# game/episode011.rpy:1210
translate german ep011_station_92ebdc52:

    # man "Information on the reward for this bounty can be found at the bounty office in section 16, room 108."
    man "Informationen über die Belohnung für dieses Kopfgeld erhalten Sie im Kopfgeldbüro in Abschnitt 16, Zimmer 108."

# game/episode011.rpy:1211
translate german ep011_station_d73d48cd:

    # man "Related to this bulletin is the following missive from the Sovereignty."
    man "Zu diesem Bulletin gehört folgendes Schreiben der Souveränität."

# game/episode011.rpy:1212
translate german ep011_station_64921016:

    # man "Wanted, dead or alive, [p_name] Valenmann de Lonval."
    man "Wanted, dead or alive, [p_name] Valenmann de Lonval."

# game/episode011.rpy:1213
translate german ep011_station_c7df0290:

    # man "Valenmann de Lonval stands accused of deserting from the Terran Galactic Navy, commandeering Sovereignty property and endangering his fellow officers."
    man "Valenmann de Lonval wird beschuldigt, von der Terranischen Galaktischen Marine desertiert zu sein, Eigentum der Souveränität beschlagnahmt und seine Offizierskollegen gefährdet zu haben."

# game/episode011.rpy:1214
translate german ep011_station_118db03b:

    # man "Travelling with him are the following people..."
    man "Folgende Personen reisen mit ihm..."

# game/episode011.rpy:1216
translate german ep011_station_6404d856:

    # "I didn’t wait for the broadcast to finish and hurried across the square towards the docks."
    "Ich wartete nicht, bis die Sendung zu Ende war, sondern eilte über den Platz zu den Docks."

# game/episode011.rpy:1217
translate german ep011_station_a73e2733:

    # "Luckily, the square was all but deserted and nobody seemed to be paying attention to the broadcast."
    "Zum Glück war der Platz fast menschenleer und niemand schien die Übertragung zu verfolgen."

# game/episode011.rpy:1218
translate german ep011_station_57b74c76:

    # "Despite this, I felt safer when the security hatch of the Bastard closed behind me."
    "Trotzdem fühlte ich mich sicherer, als sich die Sicherheitsluke des Bastard hinter mir schloss."

# game/episode011.rpy:1219
translate german ep011_station_c23db7cf:

    # c "Well fuck, this is a level of infamy we can’t really afford ourselves right now."
    c "Verdammt, so eine Schande können wir uns im Moment wirklich nicht leisten."

# game/episode011.rpy:1221
translate german ep011_station_50c550e2:

    # av "We sure can’t."
    av "Das können wir ganz sicher nicht."

# game/episode011.rpy:1222
translate german ep011_station_d29676c0:

    # av "I saw the same messages plastered all over the walls in several sectors."
    av "Ich habe in mehreren Sektoren die gleichen Botschaften überall an den Wänden gesehen."

# game/episode011.rpy:1223
translate german ep011_station_07c6d0f6:

    # c "It’s just a matter of time before someone takes an unhealthy interest and reports us to the authorities."
    c "Es ist nur eine Frage der Zeit, bis jemand ein ungesundes Interesse entwickelt und uns bei den Behörden anzeigt."

# game/episode011.rpy:1225
translate german ep011_station_1115db39:

    # th "The ship’s signature is scrambled and won’t appear as suspicious on any docking registry."
    th "Die Schiffssignatur ist verschlüsselt und wird in keinem Hafenregister als verdächtig erscheinen."

# game/episode011.rpy:1226
translate german ep011_station_fe191f93:

    # th "So we could just dock, stay inside the ship, stock up and leave again."
    th "Also könnten wir einfach anlegen, im Schiff bleiben, Vorräte auffüllen und wieder abfahren."

# game/episode011.rpy:1227
translate german ep011_station_eac0dd52:

    # c "That would work, but we can’t hide inside our ship on every space station we visit."
    c "Das würde funktionieren, aber wir können uns nicht auf jeder Raumstation, die wir besuchen, in unserem Raumschiff verstecken."

# game/episode011.rpy:1228
translate german ep011_station_c568efa4:

    # c "If we encounter some overeager station official and they conduct a search of the ship, we’re doomed."
    c "Wenn wir auf einen übereifrigen Stationsbeamten treffen und dieser das Schiff durchsucht, sind wir verloren."

# game/episode011.rpy:1232
translate german ep011_thyia_park_c0614094:

    # c "Thyia?"
    c "Thyia?"

# game/episode011.rpy:1234
translate german ep011_thyia_park_92fc0116:

    # th "Hmmm?"
    th "Hmmm?"

# game/episode011.rpy:1235
translate german ep011_thyia_park_987c4868:

    # c "If you feel there’s any unfinished business here, don’t hesitate to take the opportunity."
    c "Falls du das Gefühl hast, dass hier noch etwas zu erledigen ist, zögere nicht, die Gelegenheit zu ergreifen."

# game/episode011.rpy:1236
translate german ep011_thyia_park_727b79ce:

    # c "Aven and Céline are likely occupied for a few hours."
    c "Aven und Céline sind vermutlich für ein paar Stunden beschäftigt."

# game/episode011.rpy:1238
translate german ep011_thyia_park_4746ccbb:

    # th "What the hell would I need to do here in this shithole?"
    th "Was zum Teufel sollte ich hier in diesem Drecksloch tun?"

# game/episode011.rpy:1239
translate german ep011_thyia_park_dd29e29f:

    # c "Thyia...{w} Just think about it."
    c "Thyia...{w} Denk mal drüber nach."

# game/episode011.rpy:1240
translate german ep011_thyia_park_152a14cd:

    # "We were about to approach the hallway leading to the docks when Thyia stopped abruptly."
    "Wir waren gerade dabei, den Flur zu erreichen, der zu den Docks führte, als Thyia abrupt stehen blieb."

# game/episode011.rpy:1241
translate german ep011_thyia_park_38979362:

    # "She’d been silent, clearly lost in thought."
    "Sie hatte geschwiegen, offensichtlich in Gedanken versunken."

# game/episode011.rpy:1243
translate german ep011_thyia_park_0ad0131b:

    # th "You think I should go back to that place?"
    th "Meinst du, ich sollte dorthin zurückkehren?"

# game/episode011.rpy:1244
translate german ep011_thyia_park_796b66d5:

    # th "Where it all happened, I mean?"
    th "Ich meine, wo das alles passiert ist?"

# game/episode011.rpy:1245
translate german ep011_thyia_park_d12fc2f8:

    # c "Maybe? I’m not an expert, but it could perhaps give you some closure."
    c "Vielleicht? Ich bin kein Experte, aber es könnte dir vielleicht zu einem gewissen Abschluss verhelfen."

# game/episode011.rpy:1247
translate german ep011_thyia_park_69281a04:

    # th "I doubt it."
    th "Das bezweifle ich."

# game/episode011.rpy:1248
translate german ep011_thyia_park_0420865d:

    # th "But fuck it, let’s go have a look."
    th "Aber scheiß drauf, lass uns mal nachsehen."

# game/episode011.rpy:1250
translate german ep011_thyia_park_d430f46f:

    # "Thyia guided us unfailingly towards the place where the massacre had taken place."
    "Thyia führte uns zielsicher zu dem Ort, an dem das Massaker stattgefunden hatte."

# game/episode011.rpy:1251
translate german ep011_thyia_park_ecb68295:

    # "She hardly spoke and was way more nervous than she let on."
    "Sie sprach kaum und war viel nervöser, als sie zugab."

# game/episode011.rpy:1252
translate german ep011_thyia_park_3a79c87c:

    # "After following a long corridor deep inside the space station, we stepped out into a spacious green square."
    "Nachdem wir einen langen Korridor tief im Inneren der Raumstation entlanggegangen waren, traten wir auf einen geräumigen grünen Platz."

# game/episode011.rpy:1254
translate german ep011_thyia_park_0e45ff74:

    # "Lush green foliage covered almost half of the square, adorned with benches and even a fountain."
    "Üppiges Grün bedeckte fast die Hälfte des Platzes, der mit Bänken und sogar einem Brunnen geschmückt war."

# game/episode011.rpy:1256
translate german ep011_thyia_park_87870c54:

    # th "They turned it into a fucking park."
    th "Die haben daraus einen verdammten Park gemacht."

# game/episode011.rpy:1257
translate german ep011_thyia_park_9ab17f51:

    # c "Rather unexpected compared to the rest of Shuty Station."
    c "Ziemlich unerwartet im Vergleich zum Rest von Shuty Station."

# game/episode011.rpy:1258
translate german ep011_thyia_park_8a4b3c79:

    # th "This was all bare metal plating before, to accommodate all the market stalls."
    th "Vorher war das alles blankes Metallblech, um all die Marktstände unterzubringen."

# game/episode011.rpy:1259
translate german ep011_thyia_park_9912feb5:

    # "Thyia was even more surprised than I was."
    "Thyia war noch überraschter als ich."

# game/episode011.rpy:1261
translate german ep011_thyia_park_e006e093:

    # "Together, we wandered around the park, navigating between children playing and elderly people enjoying the green oasis."
    "Gemeinsam schlenderten wir durch den Park und bahnten uns unseren Weg zwischen spielenden Kindern und älteren Menschen, die die grüne Oase genossen."

# game/episode011.rpy:1263
translate german ep011_thyia_park_a28df09a:

    # "The heart of the park held a memorial stone."
    "Im Zentrum des Parks stand ein Gedenkstein."

# game/episode011.rpy:1264
translate german ep011_thyia_park_8879d157:

    # c "There’s a plaque here."
    c "Hier befindet sich eine Gedenktafel."

# game/episode011.rpy:1266
translate german ep011_thyia_park_3aa18fb8:

    # th "“Zuvranik Park is dedicated to the civilians who fell in the massacre of Shuty Market, 12th of August, 5391.”"
    th "„Der Zuvranik-Park ist den Zivilisten gewidmet, die beim Massaker auf dem Shuty-Markt am 12. August 5391 ums Leben kamen.“"

# game/episode011.rpy:1267
translate german ep011_thyia_park_d12fa3f5:

    # th "Fuck."
    th "Verdammt."

# game/episode011.rpy:1269
translate german ep011_thyia_park_832bcb8a:

    # "Thyia repeated the same word over and over, her eyes rimmed with tears."
    "Thyia wiederholte immer wieder dasselbe Wort, ihre Augen waren von Tränen umrandet."

# game/episode011.rpy:1271
translate german ep011_thyia_park_f01615f7:

    # "She accepted my embrace wearily, but relaxed once I held her in my arms."
    "Sie nahm meine Umarmung etwas müde entgegen, entspannte sich aber, sobald ich sie in meinen Armen hielt."

# game/episode011.rpy:1272
translate german ep011_thyia_park_d3b1490e:

    # "When the worst of it had passed, she still held me tight and stared wordlessly at the memorial stone."
    "Als das Schlimmste vorüber war, hielt sie mich immer noch fest und starrte wortlos auf den Gedenkstein."

# game/episode011.rpy:1274
translate german ep011_thyia_park_568fa77e:

    # "An elderly man shuffled next to us, regarding us curiously."
    "Ein älterer Mann schlurfte neben uns her und musterte uns neugierig."

# game/episode011.rpy:1275
translate german ep011_thyia_park_6b039c7d:

    # "I could tell he wanted to say something, but propriety held him back."
    "Ich merkte, dass er etwas sagen wollte, aber Anstand hielt ihn davon ab."

# game/episode011.rpy:1276
translate german ep011_thyia_park_4324afa6:

    # "When he’d finally mustered the courage, his croaking voice disturbed the peacefulness of the memorial."
    "Als er schließlich den Mut aufgebracht hatte, störte seine krächzende Stimme die friedliche Stille der Gedenkstätte."

# game/episode011.rpy:1279
translate german ep011_thyia_park_866bc220:

    # man "Did you lose someone dear in those fateful events back then, miss?"
    man "Haben Sie bei diesen schicksalhaften Ereignissen damals einen geliebten Menschen verloren, Miss?"

# game/episode011.rpy:1281
translate german ep011_thyia_park_8fc8608c:

    # th "Something like that."
    th "So in etwa."

# game/episode011.rpy:1283
translate german ep011_thyia_park_cddf0d97:

    # man "Nasty business that, ironically it made Zuvranik Square a better place after that."
    man "Eine üble Angelegenheit, die ironischerweise den Zuvranik-Platz danach zu einem besseren Ort gemacht hat."

# game/episode011.rpy:1285
translate german ep011_thyia_park_1c94d3ca:

    # th "Yeah, ironic..."
    th "Ja, ironisch..."

# game/episode011.rpy:1286
translate german ep011_thyia_park_e5cb5611:

    # th "Say, did they ever catch the bastards who did it?"
    th "Sag mal, haben sie die Bastarde, die das getan haben, jemals gefasst?"

# game/episode011.rpy:1288
translate german ep011_thyia_park_4752a4f0:

    # man "Oh sure."
    man "Oh ja."

# game/episode011.rpy:1289
translate german ep011_thyia_park_e770c892:

    # man "Not at first though, became a terrible mess, all the gangs fighting among each other."
    man "Allerdings nicht am Anfang, es wurde ein furchtbares Durcheinander, alle Banden bekämpften sich untereinander."

# game/episode011.rpy:1291
translate german ep011_thyia_park_6b3f68fa:

    # th "I can imagine."
    th "Das kann ich mir vorstellen."

# game/episode011.rpy:1292
translate german ep011_thyia_park_1b077f3e:

    # man "But law enforcement got them in the end."
    man "Aber die Strafverfolgungsbehörden haben sie am Ende doch noch erwischt."

# game/episode011.rpy:1293
translate german ep011_thyia_park_23032d8b:

    # man "That leader, what was his name?"
    man "Wie hieß dieser Anführer?"

# game/episode011.rpy:1294
translate german ep011_thyia_park_772998b9:

    # man "Tiberius."
    man "Tiberius."

# game/episode011.rpy:1296
translate german ep011_thyia_park_fad2aa3e:

    # th "Tyber."
    th "Tyber."

# game/episode011.rpy:1298
translate german ep011_thyia_park_10f7e9d9:

    # man "Right, Tiberius."
    man "Richtig, Tiberius."

# game/episode011.rpy:1299
translate german ep011_thyia_park_907cd80f:

    # man "They took him alive."
    man "Sie haben ihn lebend gefangen genommen."

# game/episode011.rpy:1300
translate german ep011_thyia_park_bb3c4cfd:

    # man "He stood trial and everything."
    man "Er wurde vor Gericht gestellt und alles."

# game/episode011.rpy:1301
translate german ep011_thyia_park_ee2ad0fa:

    # man "Showed no remorse whatsoever."
    man "Zeigte keinerlei Reue."

# game/episode011.rpy:1302
translate german ep011_thyia_park_44335b40:

    # man "So they spaced him."
    man "Also haben sie ihn aus dem Verkehr gezogen."

# game/episode011.rpy:1303
translate german ep011_thyia_park_8aa4c9e7:

    # man "Good riddance, if you ask me."
    man "Gut, dass du weg bist, wenn du mich fragst."

# game/episode011.rpy:1305
translate german ep011_thyia_park_ee7848a0:

    # "Clearly disappointed by the one-sidedness of the conversation, the old man ambled away, probably looking for other victims to harass."
    "Offensichtlich enttäuscht über die Einseitigkeit des Gesprächs, schlenderte der alte Mann davon, vermutlich auf der Suche nach anderen Opfern, die er schikanieren konnte."

# game/episode011.rpy:1307
translate german ep011_thyia_park_7fef7943:

    # th "Tyber is dead..."
    th "Tyber ist tot..."

# game/episode011.rpy:1309
translate german ep011_thyia_park_d6720ad8:

    # "Thyia sought my embrace again, but this time she wore a slight smile as she looked out over the park that honored the victims of the gang she once ran with."
    "Thyia suchte erneut meine Umarmung, doch diesmal trug sie ein leichtes Lächeln auf den Lippen, als sie über den Park blickte, der den Opfern der Gang gewidmet war, mit der sie einst zusammen war."

# game/episode011.rpy:1311
translate german ep011_thyia_park_a46f5771:

    # "A station announcement ripped through our mutual silence and Thyia looked up at me."
    "Eine Durchsage des Senders zerriss unsere gegenseitige Stille und Thyia blickte zu mir auf."

# game/episode011.rpy:1312
translate german ep011_thyia_park_2574661a:

    # "She looked at me as she’d never done before, like something momentous had shifted."
    "Sie sah mich an, wie sie es noch nie zuvor getan hatte, als hätte sich etwas Bedeutendes verändert."

# game/episode011.rpy:1314
translate german ep011_thyia_park_c4971635:

    # "Breaking away from my gaze, she kissed my lips, a gesture that turned into something mutual."
    "Sie wandte den Blick von mir ab und küsste meine Lippen – eine Geste, die sich zu etwas Gegenseitigem entwickelte."

# game/episode011.rpy:1315
translate german ep011_thyia_park_e7234a4b:

    # "When we broke away, Thyia was the first to speak."
    "Als wir uns abspalteten, war Thyia die Erste, die das Wort ergriff."

# game/episode011.rpy:1317
translate german ep011_thyia_park_29fd4d59:

    # th "Best not keep the others waiting."
    th "Lass die anderen am besten nicht warten."

# game/episode011.rpy:1318
translate german ep011_thyia_park_1e91c537:

    # "In that instant, I could only admire how she conveniently evaded any possible conversation about what just happened."
    "In diesem Augenblick konnte ich nur bewundern, wie geschickt sie jede mögliche Unterhaltung über das Geschehene vermieden hatte."

# game/episode011.rpy:1320
translate german ep011_thyia_park_299bd3af:

    # "She was already exiting the park towards the docking bays, a newfound spring in her step."
    "Sie verließ bereits den Park in Richtung der Anlegestellen, ihr Schritt war von neuem Schwung erfüllt."

# game/episode011.rpy:1321
translate german ep011_thyia_park_09d0fda4:

    # "I followed her, smiling, but vowing I’d confront her later with the inevitable questions we both had to answer for ourselves."
    "Ich folgte ihr lächelnd, schwor mir aber, sie später mit den unvermeidlichen Fragen zu konfrontieren, die wir beide für uns selbst beantworten mussten."

# game/episode011.rpy:1328
translate german ep011_quarters_8c393f8e:

    # "Back on the Bastard, I asked Lilly, Eva, Aven and Nadya to come to my quarters."
    "Zurück auf der Bastard bat ich Lilly, Eva, Aven und Nadya, in meine Kabine zu kommen."

# game/episode011.rpy:1329
translate german ep011_quarters_9c7636a6:

    # c "It seems everyone is looking for us."
    c "Es scheint, als ob alle nach uns suchen."

# game/episode011.rpy:1330
translate german ep011_quarters_e867c974:

    # c "Even on this backwater station they’ve plastered our faces on every screen."
    c "Selbst auf diesem abgelegenen Sender prangen unsere Gesichter auf jedem Bildschirm."

# game/episode011.rpy:1331
translate german ep011_quarters_d7fdd953:

    # c "It’s only a matter of time before someone takes an unhealthy interest."
    c "Es ist nur eine Frage der Zeit, bis jemand ein ungesundes Interesse daran entwickelt."

# game/episode011.rpy:1333
translate german ep011_quarters_0786e0cd:

    # na "Are you proposing we run?"
    na "Schlägst du vor, dass wir kandidieren?"

# game/episode011.rpy:1335
translate german ep011_quarters_6e9aed01:

    # e "Where would we go?"
    e "Wohin sollten wir gehen?"

# game/episode011.rpy:1336
translate german ep011_quarters_4982bc33:

    # e "Shuty is a G-class station, light-years away from the Sovereignty and yet they’re flooding the station’s newsfeed with wanted messages."
    e "Shuty ist ein Sender der Klasse G, Lichtjahre von der Souveränität entfernt, und trotzdem überschwemmen sie den Newsfeed des Senders mit Fahndungsmeldungen."

# game/episode011.rpy:1338
translate german ep011_quarters_5bdd9263:

    # na "I’m sure there are stations or planets that want nothing to do with the Sovereignty."
    na "Ich bin mir sicher, dass es Stationen oder Planeten gibt, die nichts mit der Souveränität zu tun haben wollen."

# game/episode011.rpy:1340
translate german ep011_quarters_fc365825:

    # av "I’m sure there are, but they generally hate all humans, regardless if they’re from the Sovereignty or not."
    av "Ich bin mir sicher, dass es welche gibt, aber im Allgemeinen hassen sie alle Menschen, egal ob sie aus der Souveränität stammen oder nicht."

# game/episode011.rpy:1341
translate german ep011_quarters_b941f26e:

    # av "And as [p_name_short] says, if there’s only one bounty hunter that takes an unhealthy interest, we’re fucked."
    av "Und wie [p_name_short] schon sagte: Wenn nur ein einziger Kopfgeldjäger ein ungesundes Interesse zeigt, sind wir am Arsch."

# game/episode011.rpy:1343
translate german ep011_quarters_ac8d6175:

    # l "Be that as it may, I don’t see the alternative here?"
    l "Wie dem auch sei, ich sehe hier keine Alternative?"

# game/episode011.rpy:1345
translate german ep011_quarters_28fab666:

    # e "I think we should take the fight to them."
    e "Ich denke, wir sollten den Kampf gegen sie aufnehmen."

# game/episode011.rpy:1347
translate german ep011_quarters_ee62018f:

    # l "Them?"
    l "Die beiden?"

# game/episode011.rpy:1348
translate german ep011_quarters_516dd4cc:

    # c "All those fuckers who’re currently after us."
    c "All diese Arschlöcher, die uns derzeit verfolgen."

# game/episode011.rpy:1350
translate german ep011_quarters_878fd03a:

    # e "Dad in particular."
    e "Insbesondere Papa."

# game/episode011.rpy:1351
translate german ep011_quarters_deea3c1b:

    # l "Dad, why?"
    l "Papa, warum?"

# game/episode011.rpy:1353
translate german ep011_quarters_e1d632e8:

    # e "Your dad in particular."
    e "Insbesondere dein Vater."

# game/episode011.rpy:1354
translate german ep011_quarters_3389aebf:

    # l "Agust, why?"
    l "Agust, warum?"

# game/episode011.rpy:1355
translate german ep011_quarters_f4ba94ee:

    # c "It seems that Dad has bought his way into the Sovereignty Council, using Eva as payment."
    c "Es scheint, als hätte sich Dad seinen Platz im Souveränitätsrat erkauft und Eva als Bezahlung benutzt."

# game/episode011.rpy:1357
translate german ep011_quarters_dd296d8a:

    # l "What?!"
    l "Was?!"

# game/episode011.rpy:1358
translate german ep011_quarters_82f31ae3:

    # c "Like the Confessor, the Council of the Sovereignty, seems to think Eva is special."
    c "Wie der Bekenner scheint auch der Rat der Souveränität zu glauben, dass Eva etwas Besonderes ist."

# game/episode011.rpy:1360
translate german ep011_quarters_f3b875b4:

    # l "But how did Dad sell Eva out?"
    l "Aber wie konnte Dad Eva verraten?"

# game/episode011.rpy:1361
translate german ep011_quarters_f4edce4c:

    # c "We’re not sure, but intend to find out."
    c "Wir sind uns nicht sicher, beabsichtigen aber, es herauszufinden."

# game/episode011.rpy:1363
translate german ep011_quarters_7bd1c610:

    # "Together with Eva, I explained everything we’d learned from Vess and the prophecy pursued by the Confessor."
    "Gemeinsam mit Eva erklärte ich alles, was wir von Vess gelernt hatten, und die Prophezeiung, der der Bekenner nachging."

# game/episode011.rpy:1364
translate german ep011_quarters_2aa66c50:

    # "Nadya was particularly interested in the prophecy, positing theories, her academic mind already working overtime."
    "Nadya interessierte sich besonders für die Prophezeiung und stellte Theorien dazu auf; ihr akademischer Verstand arbeitete bereits auf Hochtouren."

# game/episode011.rpy:1365
translate german ep011_quarters_a594136b:

    # "After the torrent of questions abated, they all looked to Eva and me questioningly."
    "Nachdem der Fragenstrom abgeebbt war, blickten alle Eva und mich fragend an."

# game/episode011.rpy:1367
translate german ep011_quarters_fd0c9924:

    # av "You said you wanted to take the fight to them, Eva, what does that mean exactly."
    av "Du sagtest, du wolltest den Kampf zu ihnen tragen, Eva, was genau bedeutet das?"

# game/episode011.rpy:1369
translate german ep011_quarters_73d1aee3:

    # e "Until now, we were a witness after the fact, because the other side knows more than we do."
    e "Bis jetzt waren wir nur Zeugen im Nachhinein, weil die Gegenseite mehr weiß als wir."

# game/episode011.rpy:1370
translate german ep011_quarters_489af96d:

    # e "We can keep running, evading capture, but there will be a time we slip up and get caught."
    e "Wir können weiter rennen und der Gefangennahme entgehen, aber es wird der Tag kommen, an dem wir einen Fehler machen und erwischt werden."

# game/episode011.rpy:1372
translate german ep011_quarters_2ba85c42:

    # av "The universe is a large place though..."
    av "Das Universum ist aber riesig..."

# game/episode011.rpy:1373
translate german ep011_quarters_bff5ab0f:

    # c "That might be true, but civilized space is already a hell of a lot smaller."
    c "Das mag ja stimmen, aber der zivilisierte Raum ist schon um ein Vielfaches kleiner."

# game/episode011.rpy:1374
translate german ep011_quarters_26b7ef62:

    # c "The intelligence apparatus of the Sovereignty is also not to be underestimated."
    c "Auch der Geheimdienstapparat der Souveränität ist nicht zu unterschätzen."

# game/episode011.rpy:1375
translate german ep011_quarters_47b89796:

    # c "I agree with Eva, that we need to know more, we need to be on the same level as our pursuers are."
    c "Ich stimme Eva zu, dass wir mehr wissen müssen, wir müssen auf dem gleichen Niveau sein wie unsere Verfolger."

# game/episode011.rpy:1376
translate german ep011_quarters_df582e8e:

    # c "We need to know why Eva is so important to them, what they hope to gain."
    c "Wir müssen wissen, warum Eva ihnen so wichtig ist, was sie sich davon erhoffen."

# game/episode011.rpy:1377
translate german ep011_quarters_25c90759:

    # c "I assume it’s something powerful, I don’t really know what else a bunch of corrupt Sovereignty politicians would be interested in."
    c "Ich nehme an, es ist etwas Mächtiges, ich wüsste sonst nicht, woran eine Gruppe korrupter Souveränitätspolitiker interessiert sein sollte."

# game/episode011.rpy:1378
translate german ep011_quarters_3f76c5ec:

    # c "We could ultimately wield that power ourselves, turn the tables on them."
    c "Wir könnten diese Macht letztendlich selbst ausüben und den Spieß umdrehen."

# game/episode011.rpy:1380
translate german ep011_quarters_6ea27b09:

    # na "I do hear a lot of conjecture."
    na "Ich höre tatsächlich viele Spekulationen."

# game/episode011.rpy:1382
translate german ep011_quarters_e8ece140:

    # av "How do we accomplish all this?"
    av "Wie können wir das alles erreichen?"

# game/episode011.rpy:1385
translate german ep011_quarters_6982616d:

    # e "The first step should be confronting Dad."
    e "Der erste Schritt sollte die Konfrontation mit deinem Vater sein."

# game/episode011.rpy:1387
translate german ep011_quarters_61934bce:

    # e "The first step should be confronting [p_name_short]’s Dad."
    e "Der erste Schritt sollte die Konfrontation mit [p_name_short]s Vater sein."

# game/episode011.rpy:1388
translate german ep011_quarters_177b3aae:

    # e "The Confessor is too great an unknown and he doesn’t seem all that reasonable."
    e "Der Beichtvater ist ein zu großes Unbekanntes und er wirkt nicht besonders vernünftig."

# game/episode011.rpy:1390
translate german ep011_quarters_7fdca29a:

    # l "And Dad is?"
    l "Und Papa ist?"

# game/episode011.rpy:1392
translate german ep011_quarters_07751dbe:

    # l "And his dad is?"
    l "Und sein Vater ist?"

# game/episode011.rpy:1393
translate german ep011_quarters_c954c679:

    # e "No, but we know him."
    e "Nein, aber wir kennen ihn."

# game/episode011.rpy:1394
translate german ep011_quarters_5cad1f5a:

    # l "Not that well apparently, considering his secret career on the Council."
    l "Offenbar nicht so gut, wenn man seine geheime Karriere im Stadtrat bedenkt."

# game/episode011.rpy:1395
