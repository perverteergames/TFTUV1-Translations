    av "Warum kommst du nicht mit mir auf den Balkon?"

# game/episode008.rpy:869
translate german ep008_aven_erigone_talk_2bd8cee6:

    # av "There's booze."
    av "Es gibt Alkohol."

# game/episode008.rpy:870
translate german ep008_aven_erigone_talk_29403d57:

