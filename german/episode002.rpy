# game/episode002.rpy:44
translate german episode002_fab7838e:

    # ki "Can't we outrun them?"
    ki "Können wir nicht entkommen?"

# game/episode002.rpy:46
translate german episode002_2c9e0966:

    # ce "Not at this speed."
    ce "Nicht bei dieser Geschwindigkeit."

# game/episode002.rpy:47
translate german episode002_ffbde6e6:

    # ce "We could..."
    ce "Wir könnten..."

# game/episode002.rpy:48
